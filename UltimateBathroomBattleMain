plr:Button("Apply Upgrades", function()
local args = {
    [1] = "Upgrades",
    [2] = {
        ["PunchDamage"] = punchdmg,
        ["HealthUpgrade"] = healup,
        ["RaygunDamage"] = raydmg
    }
}
game.ReplicatedStorage.UIRemotes.SaveData:InvokeServer(unpack(args))
Notify({
Description = "Upgraded(PunchDamage: "..punchdmg..", HealthUpgrade: "..healup..", RaygunDamage: "..raydmg.. ").";
Duration = 5;
});
end)

plr:Toggle("Free Enable Gadgets", false, function(gadg)
if gadg then
local args = {
    [1] = "Gadgets",
    [2] = {
        ["BathroomFlashlight"] = true,
        ["glassesss"] = true,
        ["Tier3Armor"] = true,
        ["Tier2Armor"] = true,
        ["Tier1Armor"] = true,
        ["SpiderMinigun"] = true,
        ["Shocks"] = true,
        ["JetPack"] = true,
        ["BathroomHeadphones"] = true,
        ["Tier4Armor"] = true,
        ["BathroomCar"] = true,
        ["MetalPlate"] = true,
        ["Tier5Armor"] = true
    }
}
game.ReplicatedStorage.UIRemotes.SaveData:InvokeServer(unpack(args))
Notify({
Description = "Enabled All Gadgets";
Duration = 2;
});
else
local args = {
    [1] = "Gadgets",
    [2] = {
        ["BathroomFlashlight"] = false,
        ["glassesss"] = false,
        ["Tier3Armor"] = false,
        ["Tier2Armor"] = false,
        ["Tier1Armor"] = false,
        ["SpiderMinigun"] = false,
        ["Shocks"] = false,
        ["JetPack"] = false,
        ["BathroomHeadphones"] = false,
        ["Tier4Armor"] = false,
        ["BathroomCar"] = false,
        ["MetalPlate"] = false,
        ["Tier5Armor"] = false
    }
}
game.ReplicatedStorage.UIRemotes.SaveData:InvokeServer(unpack(args))
Notify({
Description = "Disabled All Gadgets";
Duration = 2;
});
end
end)

