ai:Button("UnGod Everyone", function()
for i, v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer and v.Character ~= nil and v.Character:FindFirstChild("wefewfegyerw8gerwrgtiju45ty845") and v.Character:FindFirstChild("GivenGod") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, v.Character)
if v.Character.HumanoidRootPart ~= nil and v.Character.HumanoidRootPart:FindFirstChild("Client_IsGod_Icon") then
v.Character.HumanoidRootPart:FindFirstChild("Client_IsGod_Icon"):Destroy()
end
if v.Character:FindFirstChild("GivenGod") then
v.Character:FindFirstChild("GivenGod"):Destroy()
end
end
end
end)

ai:Button("Check Exploiters", function()
for i, v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer then
local trigger = false
local shards = "No Data"
local frags = "No Data"
local energy = "No Data"
local candy = "No Data"
if v:FindFirstChild("Shards") then
shards = v:FindFirstChild("Shards").Value
if shards >= 500 then
trigger = true
end
end
if v:FindFirstChild("Fragments") then
frags = v:FindFirstChild("Fragments").Value
if frags >= 300 then
trigger = true
end
end
if v:FindFirstChild("Energy") then
energy = v:FindFirstChild("Energy").Value
if energy >= 1500 then
trigger = true
end
end
if v:FindFirstChild("CandyCorn") then
candy = v:FindFirstChild("CandyCorn").Value
if candy >= 5000 then
trigger = true
end
end
if trigger == false then
print("[SCRIPT] "..v.Name.."'s Data: Shards("..shards.."), Fragments("..frags.."), Energy("..energy.."), Candy Corn("..candy..").")
else
warn("[SCRIPT] "..v.Name.."'s Data: Shards("..shards.."), Fragments("..frags.."), Energy("..energy.."), Candy Corn("..candy..").")
end
end
end
end)

ai:Toggle("Bypass Remover", false, function(bool)
if bool then
local killed = 0
for i, v in pairs(workspace.Debris:GetChildren()) do
if v:IsA("Part") and v.Name == "Part" and v.Anchored and v:FindFirstChild("Value") and v.Value.Value == 0 then
game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(v, false)
killed += 1
end
end
if killed > 0 then print("[SCRIPT] Just Destroyed "..killed.." Parts with Value found (Bypassed)") end
_G.UBBRemover = workspace.Debris.ChildAdded:Connect(function(child)
if child:IsA("Part") and child.Name == "Part" and child.Anchored and child:FindFirstChild("Value") and child.Value.Value == 0 then
game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(child, false)
print("[SCRIPT] Just Destroyed a Part with Value found (Bypassed)")
end
end)
else
if _G.UBBRemover then _G.UBBRemover:Disconnect() end
end
end)

