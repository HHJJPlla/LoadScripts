G2L["8"]["Color"] = Color3.fromRGB(0, 99, 225)
game.Players.LocalPlayer.Character["GMan5Attacks"]["Misc_Dizzy"].Value = false
end)

_G.GMan5ControlButtons = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
if input.UserInputType == Enum.UserInputType.MouseButton1 and gameProcessed == false and game.Players.LocalPlayer.Character["GMan5Attacks"]["Invisible_Status"].Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].Punch.Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value == false and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
if math.random(1, 2) == 1 then
game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value = true
game.Players.LocalPlayer.Character["GMan5Attacks"].Punch.Value = true
if game.Players.LocalPlayer.Character["GMan5Attacks"].Escape.Value == false then
FlySpeed = 250
end
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.HumanoidRootPart["ValveBiped.Bip01_Pelvis"]["arm right shoulder 2"]["arm shoulder 2 twist right"]["arm right elbow"]["r_bottom engine"], true)
local pucher1 = game.Players.LocalPlayer.Character.m1hitbox.Position
task.spawn(function()
punchplay:Play()
end)
task.wait(0.2)
if GMan5Alive == false then return end
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "FastHandMovement"})
for i, v in pairs(game.Players:GetPlayers()) do
	if v.Character ~= nil and v.Character:FindFirstChild("LensmenTeam") and v.Character.PrimaryPart and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 and (pucher1-v.Character.PrimaryPart.Position).Magnitude <= 90 then
		game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.BOSS.M1Hit, v.Character.PrimaryPart.CFrame, "jug2")
		game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.SOUNDWOMAN.Cloud, v.Character.PrimaryPart.CFrame, "ImpactorImpact")
		if string.match(v.Character.Name, "Giant") and not v.Character:FindFirstChild("RagdolledCustom") and v.Character.Humanoid.Health-(2000+StrengthAddon)>0 then		
			task.spawn(function()
				local thrown = v.Character
				local ragdolled = Instance.new("BoolValue", thrown)
				ragdolled.Name = "RagdolledCustom"
				game.ReplicatedStorage.CombatRemotes.MagnetHand:FireServer(thrown.PrimaryPart, true, thrown.Humanoid)
				game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {thrown.PrimaryPart, "RagdollerFly"})
				game.ReplicatedStorage.CombatRemotes.BathroomReworkFiles:FireServer("knockmalformed", thrown, game.Players.LocalPlayer.Character)
				game.ReplicatedStorage.CombatRemotes.BathroomReworkFiles:FireServer("knockmalformed", thrown, game.Players.LocalPlayer.Character)
				task.wait(3)
				if thrown ~= nil and thrown:FindFirstChild("HumanoidRootPart") and thrown:FindFirstChild("Humanoid") then
					game.ReplicatedStorage.CombatRemotes.MagnetHand:FireServer(thrown.PrimaryPart, false, thrown.Humanoid)
					game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.BOSS.OIOIOI, thrown.PrimaryPart.CFrame, "RagdollerLand1")
					task.wait(8)
					if thrown ~= nil and thrown.PrimaryPart and thrown:FindFirstChild("Humanoid") then
						ragdolled:Destroy()
					end
				end
			end)
		end
		game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Character.Humanoid, 2000 + StrengthAddon, false, true)
		break
	end
end
task.wait(0.5)
