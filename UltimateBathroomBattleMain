local BazookaLock = false

bindas:Toggle("Bazooka-G Control", false, function(bool)
if bool then
G2L["1"].Enabled = true
if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BazookaGiant" then
if not game.Players.LocalPlayer.Character:FindFirstChild("Attacks") then
local folder = Instance.new("Folder", game.Players.LocalPlayer.Character)
folder.Name = "Attacks"
local acidrocketslaunch = Instance.new("Animation", folder)
acidrocketslaunch.AnimationId = "rbxassetid://121031420060256"
acidrocketslaunch.Name = "AcidFire"
Instance.new("BoolValue", folder).Name = "RocketCD"
Instance.new("BoolValue", folder).Name = "DoingShield"
Instance.new("BoolValue", folder).Name = "DoingShieldCD"
Instance.new("BoolValue", folder).Name = "DoSprint"
local blockanim = Instance.new("Animation", folder)
blockanim.AnimationId = "rbxassetid://120443927143103"
blockanim.Name = "Block"
local runningAnim = Instance.new("Animation", folder)
runningAnim.AnimationId = "rbxassetid://100088825029357"
runningAnim.Name = "Run"
end
if not workspace:FindFirstChild("ToServerSound") then
local toserversd = Instance.new("Folder", workspace)
toserversd.Name = "ToServerSound"
game.Players.LocalPlayer:GetMouse().TargetFilter = toserversd
end
if game.Players.LocalPlayer.Character:FindFirstChild("BazookaGiantControl") then
game.Players.LocalPlayer.Character.BazookaGiantControl.Disabled = true
end
end
_G.BazookaControlButtons = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
if input.KeyCode == Enum.KeyCode.Z and gameProcessed == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BazookaGiant" and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoSprint").Value = not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoSprint").Value
if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoSprint").Value then
Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks).Name = "cannotBlock"
G2L["i"]["BackgroundColor3"] = Color3.fromRGB(0, 180, 0)
G2L["j"]["Color"] = Color3.fromRGB(0, 150, 0)
local runanim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.Run)
runanim.Name = "RunningAnim"
_G.SprintDetBazookaG = game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("MoveDirection"):Connect(function()
if game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Magnitude > 0 and game.Players.LocalPlayer.Character.Humanoid.FloorMaterial ~= Enum.Material.Air then
if game.Players.LocalPlayer.Character:FindFirstChild("isSprinting") then return end
runanim:Play()
runanim:AdjustSpeed(0.6)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 80
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
Instance.new("BoolValue", game.Players.LocalPlayer.Character).Name = "isSprinting"
else
if game.Players.LocalPlayer.Character:FindFirstChild("isSprinting") then
runanim:Stop(0.1)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 30
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
game.Players.LocalPlayer.Character:FindFirstChild("isSprinting"):Destroy()
end
end
end)
else
G2L["i"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0)
G2L["j"]["Color"] = Color3.fromRGB(225, 0, 0)
if _G.SprintDetBazookaG then
_G.SprintDetBazookaG:Disconnect()
    for _, v in ipairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
        if v.Name == "RunningAnim" then
            v:Stop(0.1)
            v:Destroy()
        end
    end
end
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 30
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannotBlock"):Destroy()
if game.Players.LocalPlayer.Character:FindFirstChild("isSprinting") then
game.Players.LocalPlayer.Character:FindFirstChild("isSprinting"):Destroy()
end
end
end
if input.KeyCode == Enum.KeyCode.R and gameProcessed == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BazookaGiant" and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShieldCD").Value == false then
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShieldCD").Value = true
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShield").Value = not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShield").Value
if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShield").Value == true then
G2L["e"]["BackgroundColor3"] = Color3.fromRGB(0, 180, 0)
G2L["f"]["Color"] = Color3.fromRGB(0, 150, 0)
Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks).Name = "cannot"
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", true, game.Players.LocalPlayer.Character)
local blockanimplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.Block)
blockanimplay.Name = "BlockingAnim"
blockanimplay:Play(0.2)
blockanimplay:AdjustSpeed(0)
task.wait(2)
if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShieldCD") then
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShieldCD").Value = false
end
else
G2L["e"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0)
G2L["f"]["Color"] = Color3.fromRGB(225, 0, 0)
if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot"):Destroy()
end
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", false, game.Players.LocalPlayer.Character)
    for _, v in ipairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
        if v.Name == "BlockingAnim" then
            v:Stop(0.2)
            v:Destroy()
        end
    end
task.wait(2)
if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShieldCD") then
game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoingShieldCD").Value = false
end
end
end
	if input.KeyCode == Enum.KeyCode.E and gameProcessed == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BazookaGiant" and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
		BazookaLock = not BazookaLock
		if BazookaLock then
			G2L["a"]["BackgroundColor3"] = Color3.fromRGB(0, 180, 0)
			G2L["b"]["Color"] = Color3.fromRGB(0, 150, 0)
		else
			G2L["a"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0)
			G2L["b"]["Color"] = Color3.fromRGB(225, 0, 0)
		end
	end
	if input.KeyCode == Enum.KeyCode.Q and gameProcessed == false and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BazookaGiant" and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
		if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("RocketCD") then
			Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks).Name = "RocketCD"
			if not workspace:FindFirstChild("ToServerSound") then
				local toserversd = Instance.new("Folder", workspace)
				toserversd.Name = "ToServerSound"
				game.Players.LocalPlayer:GetMouse().TargetFilter = toserversd
			end
		end
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("RocketCD").Value == false then
			if BazookaLock == false then
				G2L["5"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0);
				G2L["6"]["Color"] = Color3.fromRGB(225, 0, 0);
				if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("AcidFire") then
					local acidrocketslaunch = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
					acidrocketslaunch.AnimationId = "rbxassetid://121031420060256"
					acidrocketslaunch.Name = "AcidFire"
				end
				if game.Players.LocalPlayer:GetMouse().Target ~= nil then
					game.Players.LocalPlayer.Character.Attacks:FindFirstChild("RocketCD").Value = true
					game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "Shoot"})
					local arlaunchplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks:FindFirstChild("AcidFire"))
					arlaunchplay:Play()
					local soundpart = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.SOUNDWOMAN.Cloud, game.Players.LocalPlayer:GetMouse().Hit.Position)
					soundpart.Anchored = true
					soundpart.Parent = workspace:FindFirstChild("ToServerSound")
					task.spawn(function()
						local PlayerPos = game.Players.LocalPlayer.Character["w_rpg_reference.smd"].Attachment.WorldPosition
						local RocketClone = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").acidrocket, PlayerPos)
						local bodyvel = Instance.new("BodyVelocity", RocketClone)
						bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
						bodyvel.Velocity = Vector3.new(0, 0, 0)
						local MouseCFrame = game.Players.LocalPlayer:GetMouse().Hit.Position
						local CalcMagnitude = (PlayerPos - MouseCFrame).Magnitude
						local CalcDistance = (PlayerPos - MouseCFrame) / 2
						local DevideMagnitude = CalcMagnitude / 2
						local calcArg3 = CFrame.new((CFrame.new(PlayerPos)) * (CalcDistance / -20)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
						local calcArg4 = CFrame.new((CFrame.new(MouseCFrame)) * (CalcDistance / 20)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
						RocketClone.Position = PlayerPos
						RocketClone.Anchored = false
						RocketClone.CanCollide = false
						RocketClone.Massless = true
						for loopMath = 5, CalcMagnitude, 4 do
							if RocketClone then
								local newMag = loopMath / CalcMagnitude
								RocketClone.CFrame = RocketClone.CFrame:Lerp(CFrame.new(lerpModule:cubicBezier(newMag, PlayerPos, calcArg3, calcArg4, MouseCFrame), MouseCFrame), newMag)
								game:GetService("RunService").Heartbeat:Wait()
							else
								break
							end
						end
						game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RocketClone, true)
						RocketClone:WaitForChild("STARMANBUFF", 5)
						if RocketClone then
							game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ROCKETS.EXPLODE, RocketClone.CFrame)
							if soundpart then
								game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {soundpart, "PlagueBoom"..math.random(1, 4)})
							end
							for i, v in pairs(workspace:GetChildren()) do
								if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (RocketClone.Position-v.HumanoidRootPart.Position).Magnitude <= 110 then
									game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 2000 - math.random(0, 1000), false, false, "Rocket")
								end
							end
							game:GetService("ReplicatedStorage").CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(RocketClone)
						end
					end)
					task.spawn(function()
						local PlayerPos = game.Players.LocalPlayer.Character["w_rpg_reference.smd.001"].Attachment.WorldPosition
						local RocketClone = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").acidrocket, PlayerPos)
						local bodyvel = Instance.new("BodyVelocity", RocketClone)
						bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
						bodyvel.Velocity = Vector3.new(0, 0, 0)
						local MouseCFrame = game.Players.LocalPlayer:GetMouse().Hit.Position
						local CalcMagnitude = (PlayerPos - MouseCFrame).Magnitude
						local CalcDistance = (PlayerPos - MouseCFrame) / 2
						local DevideMagnitude = CalcMagnitude / 2
						local calcArg3 = CFrame.new((CFrame.new(PlayerPos)) * (CalcDistance / -20)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
						local calcArg4 = CFrame.new((CFrame.new(MouseCFrame)) * (CalcDistance / 20)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
						RocketClone.Position = PlayerPos
						RocketClone.Anchored = false
						RocketClone.CanCollide = false
						RocketClone.Massless = true
						for loopMath = 5, CalcMagnitude, 4 do
							if RocketClone then
								local newMag = loopMath / CalcMagnitude
								RocketClone.CFrame = RocketClone.CFrame:Lerp(CFrame.new(lerpModule:cubicBezier(newMag, PlayerPos, calcArg3, calcArg4, MouseCFrame), MouseCFrame), newMag)
								game:GetService("RunService").Heartbeat:Wait()
							else
								break
							end
						end
						game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RocketClone, true)
						RocketClone:WaitForChild("STARMANBUFF", 5)
						if RocketClone then
							game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ROCKETS.EXPLODE, RocketClone.CFrame)
							if soundpart then
								game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {soundpart, "PlagueBoom"..math.random(1, 4)})
							end
							for i, v in pairs(workspace:GetChildren()) do
								if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (RocketClone.Position-v.HumanoidRootPart.Position).Magnitude <= 110 then
									game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 2000 - math.random(0, 1000), false, false, "Rocket")
								end
							end
							game:GetService("ReplicatedStorage").CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(RocketClone)
						end
					end)
				end
				task.wait(3)
				G2L["5"]["BackgroundColor3"] = Color3.fromRGB(0, 180, 0);
				G2L["6"]["Color"] = Color3.fromRGB(0, 150, 0);
				if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("RocketCD") then
					game.Players.LocalPlayer.Character.Attacks:FindFirstChild("RocketCD").Value = false
				end
			else
				local nearestEnt = GetNearestPlayer(500, game.Players.LocalPlayer.Character.HumanoidRootPart, "LensmenTeam", false)
				if nearestEnt then
					game.Players.LocalPlayer.Character.Attacks:FindFirstChild("RocketCD").Value = true
					G2L["5"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0);
					G2L["6"]["Color"] = Color3.fromRGB(225, 0, 0);
					game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "Shoot"})
					local arlaunchplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks:FindFirstChild("AcidFire"))
					arlaunchplay:Play()
					local soundpart = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.SOUNDWOMAN.Cloud, nearestEnt.HumanoidRootPart.Position)
					soundpart.Anchored = true
					soundpart.Parent = workspace:FindFirstChild("ToServerSound")
					task.spawn(function()
						local PlayerPos = game.Players.LocalPlayer.Character["w_rpg_reference.smd"].Attachment.WorldPosition
						local RocketClone = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").acidrocket, PlayerPos)
						local bodyvel = Instance.new("BodyVelocity", RocketClone)
						bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
						bodyvel.Velocity = Vector3.new(0, 0, 0)
						local MouseCFrame = nearestEnt.HumanoidRootPart.Position
						local CalcMagnitude = (PlayerPos - MouseCFrame).Magnitude
						local CalcDistance = (PlayerPos - MouseCFrame) / 2
						local DevideMagnitude = CalcMagnitude / 2
						local calcArg3 = CFrame.new((CFrame.new(PlayerPos)) * (CalcDistance / -20)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
						local calcArg4 = CFrame.new((CFrame.new(MouseCFrame)) * (CalcDistance / 20)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
						RocketClone.Position = PlayerPos
						RocketClone.Anchored = false
						RocketClone.CanCollide = false
						RocketClone.Massless = true
						for loopMath = 5, CalcMagnitude, 4 do
							if RocketClone then
								local newMag = loopMath / CalcMagnitude
