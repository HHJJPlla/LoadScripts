    if clockproj then
        local newMag = loopMath / CalcMagnitude
	    clockproj.CFrame = clockproj.CFrame:Lerp(CFrame.new(lerpModule:cubicBezier(newMag, PlayerPos, calcArg3, calcArg4, MouseCFrame), MouseCFrame), newMag)
        game:GetService("RunService").Heartbeat:Wait()
    else
        break
    end
end
end
