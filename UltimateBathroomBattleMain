local CalcMagnitude = (PlayerPos - MouseCFrame).Magnitude
local CalcDistance = (PlayerPos - MouseCFrame) / 2
local DevideMagnitude = CalcMagnitude / 2
local calcArg3 = CFrame.new((CFrame.new(PlayerPos)) * (CalcDistance / -1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
local calcArg4 = CFrame.new((CFrame.new(MouseCFrame)) * (CalcDistance / 1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
