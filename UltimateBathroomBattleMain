bindas:Toggle("Old Jug Control", false, function(bool)
if bool then
Instance.new("BoolValue", game.Players.LocalPlayer).Name = "JugOldAntiWE"
_G.JugAddedWE = game.Players.LocalPlayer.WE.ChildAdded:Connect(function(child)
	if game.Players.LocalPlayer:FindFirstChild("JugOldAntiWE") and game.Players.LocalPlayer.Character.Name == "AstroJuggernaut" then
		if child.Name ~= "Warp" then
			game:GetService("ContextActionService"):UnbindAction(child.Name)
			child:Destroy()
		end
	end
end)
_G.ifnewjug = game.Players.LocalPlayer.CharacterAdded:Connect(function()
	if game.Players.LocalPlayer.Character.Name == "AstroJuggernaut" then
		game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz.Parent = game.Players.LocalPlayer.PlayerScripts
		game.Players.LocalPlayer.Character:WaitForChild("IN:NONE", 5)
		repeat task.wait() until not game.Players.LocalPlayer.Character:FindFirstChild("IN:NONE")
game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.PrimaryPart, "juggernaut_idleR")
		Instance.new("BoolValue", game.Players.LocalPlayer.Character).Name = "Old_XJug"
		local Attacks = Instance.new("Folder", game.Players.LocalPlayer.Character)
		Attacks.Name = "Attacks"
		Instance.new("BoolValue", Attacks).Name = "GlobalCooldown"
		Instance.new("BoolValue", Attacks).Name = "Punch"
		Instance.new("BoolValue", Attacks).Name = "ShieldPunch"
		Instance.new("BoolValue", Attacks).Name = "AstroCannon"
		Instance.new("BoolValue", Attacks).Name = "RapidFire"
		Instance.new("BoolValue", Attacks).Name = "Speedblitz"
		Instance.new("BoolValue", Attacks).Name = "AstroShield"
		Instance.new("BoolValue", Attacks).Name = "JuggernautSight"
		Instance.new("BoolValue", Attacks).Name = "Warp"
		game.Players.LocalPlayer.Character:WaitForChild("Humanoid", 10)
		local IdleJug = Instance.new("Animation")
		IdleJug.AnimationId = "rbxassetid://97211200088578"
		local IdleJugpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(IdleJug)
		IdleJugpl:Play()
		local IdleWheel = Instance.new("Animation")
		IdleWheel.AnimationId = "rbxassetid://125338685420074"
		local IdleWheelpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(IdleWheel)
		IdleWheelpl:Play()
game:GetService("Players").LocalPlayer.PlayerScripts.Binds.hudinit:Fire(game:GetService("StarterGui").MainUI.specialchoose.ASTRO.ViewportFrame.WorldModel.AstroJuggernaut, true, true)
		task.spawn(function()
			task.wait(5)
			for i, v in pairs(workspace:GetChildren()) do
				if v:IsA("Model") and v:GetChildren()[0] == nil and string.match(v.Name, "{") then
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", v, Vector3.new(0,-600,0))
				end
			end
		end)
		_G.jugDied = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
			if workspace.Debris:FindFirstChild("OldJuggyProps") and workspace.Debris.OldJuggyProps:FindFirstChild("Shield") then
				game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(workspace.Debris.OldJuggyProps.Shield)
			end
			game.Players.LocalPlayer.PlayerScripts.DoAttackz.Parent = game.Players.LocalPlayer.PlayerScripts.Binds
			if game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then
				game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug"):Destroy()
			end
			_G.jugDied:Disconnect()
		end)
	end
end)
_G.OldJugControlButtons = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
	if input.UserInputType == Enum.UserInputType.MouseButton1 and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false and game.Players.LocalPlayer.Character.Attacks.ShieldPunch.Value == false and game.Players.LocalPlayer.Character.Attacks.Punch.Value == false then
		game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = true
		game.Players.LocalPlayer.Character.Attacks.Punch.Value = true
		local PunchAnim = Instance.new("Animation")
		PunchAnim.AnimationId = "rbxassetid://95257089939643"
		local PunchAnimpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(PunchAnim)
		PunchAnimpl.Priority = Enum.AnimationPriority.Action4
		PunchAnimpl:Play()
		PunchAnimpl:AdjustSpeed(0.6)
		task.wait(0.16)
		game.Players.LocalPlayer.Character.m1hitbox.CanTouch = true
		punchHitDetect = game.Players.LocalPlayer.Character.m1hitbox.Touched:Connect(function(hit)
			if hit:FindFirstAncestorWhichIsA("Model") and not hit:FindFirstAncestorWhichIsA("Model"):FindFirstChild("AstroTeam") and hit:FindFirstAncestorWhichIsA("Model"):FindFirstChild("Humanoid") and hit:FindFirstAncestorWhichIsA("Model").Humanoid.Health > 0 and hit:FindFirstAncestorWhichIsA("Model").Parent == workspace then
				local target = hit:FindFirstAncestorWhichIsA("Model")
				if target:FindFirstChild("LensmenTeam") or target:FindFirstChild("BathroomTeam") then
					game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(target.Humanoid, 1500, false, false)
					game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ASTRO.M1Hit, target.PrimaryPart.CFrame, "BigExplosion")
					game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(target.PrimaryPart, "jug1")
				end
				game.Players.LocalPlayer.Character.m1hitbox.CanTouch = false
				punchHitDetect:Disconnect()
			end
		end)
		task.spawn(function()
			task.wait(0.35)
			if punchHitDetect then game.Players.LocalPlayer.Character.m1hitbox.CanTouch = false punchHitDetect:Disconnect() end
		end)
		task.wait(0.35)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = false
		task.wait(0.7)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.Punch.Value = false
	end
	if input.KeyCode == Enum.KeyCode.C and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false and game.Players.LocalPlayer.Character.Attacks.AstroCannon.Value == false then
		game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = true
		game.Players.LocalPlayer.Character.Attacks.AstroCannon.Value = true
		game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks, "physcannon_charge-_1_")
		local ChargeAnim = Instance.new("Animation")
		ChargeAnim.AnimationId = "rbxassetid://101251629064303"
		local ChargeAnimpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(ChargeAnim)
		ChargeAnimpl.Priority = Enum.AnimationPriority.Action4
		ChargeAnimpl:Play()
		task.delay(0.5, function()
			if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
			game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks, "load")
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Beam1, true)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment1, true)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment2, true)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].ligt, true)
		end)
		task.delay(1.1, function()
			if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
			game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks, "load")
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Beam2, true)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment3, true)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment4, true)
		end)
		task.wait(1.5)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		ChargeAnimpl:Stop()
		local ShootAnim = Instance.new("Animation")
		ShootAnim.AnimationId = "rbxassetid://130017826561798"
		local ShootAnimpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(ShootAnim)
		ShootAnimpl.Priority = Enum.AnimationPriority.Action4
		ShootAnimpl:Play()
		task.wait(0.2)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Beam1, false)
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment1, false)
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment2, false)
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].ligt, false)
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Beam2, false)
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment3, false)
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks["gems_low.smd"].Attachment4, false)
		local projJug = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.ASTRO.ProjJug, game.Players.LocalPlayer.Character.gun.gunrotateliiks.Position)
		projJug.CFrame = CFrame.new(projJug.Position, game.Players.LocalPlayer:GetMouse().Hit.Position)
		local bodyvel = Instance.new("BodyVelocity", projJug)
		bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
		bodyvel.Velocity = projJug.CFrame.LookVector * 300
		local hit = false
		projJug.CanTouch = true
		projJug.Touched:Connect(function(hitObj)
			if hit == false and not hitObj:IsDescendantOf(workspace.Debris) then
				for i, v in pairs(workspace:GetChildren()) do
					if v:IsA("Model") and not v:FindFirstChild("AstroTeam") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.PrimaryPart and (projJug.Position-v.PrimaryPart.Position).Magnitude <= 105 then
						if v:FindFirstChild("LensmenTeam") or v:FindFirstChild("BathroomTeam") then
							game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 3500, false, false)
						end
					end
				end
				game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ASTRO.Boom, projJug.CFrame, "electric_explosion1_1_1")
				game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(projJug)
			end
			hit = true
		end)
		task.wait(0.5)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = false
		task.wait(9.5)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.AstroCannon.Value = false
	end
	if input.KeyCode == Enum.KeyCode.V and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false and game.Players.LocalPlayer.Character.Attacks.RapidFire.Value == false then
		game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = true
		game.Players.LocalPlayer.Character.Attacks.RapidFire.Value = true
		local rapidFireAnim = Instance.new("Animation")
		rapidFireAnim.AnimationId = "rbxassetid://85648763510078"
		local rapidFireAnimpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(rapidFireAnim)
		rapidFireAnimpl.Priority = Enum.AnimationPriority.Action4
		rapidFireAnimpl:Play()
		rapidFireAnimpl:GetMarkerReachedSignal("shoot"):Connect(function()
			local rapidFireProj = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.ASTRO.rapidfirebul, game.Players.LocalPlayer.Character.gun.gunrotateliiks.Position)
			rapidFireProj.CanCollide = false
			rapidFireProj.CFrame = CFrame.new(rapidFireProj.Position, game.Players.LocalPlayer:GetMouse().Hit.Position)
			local bodyvel = Instance.new("BodyVelocity", rapidFireProj)
			bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
			bodyvel.Velocity = rapidFireProj.CFrame.LookVector * 200
			game:GetService("ReplicatedStorage").CombatRemotes.EmitExsisting:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks.Shootpos, "GalaxyImpact"..math.random(1, 4))
			game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.gun.gunrotateliiks, "JuggernautRapidFire")
			local hit = false
			rapidFireProj.Touched:Connect(function(hitObj)
				if hit == false and not hitObj:IsDescendantOf(workspace.Debris) then
					hit = true
					game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ASTRO.redorbexplodebig2, rapidFireProj.CFrame + Vector3.new(0, 13, 0), "StriderExplode")
					for i, v in pairs(workspace:GetChildren()) do
						if v:IsA("Model") and not v:FindFirstChild("AstroTeam") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.PrimaryPart and (rapidFireProj.Position-v.PrimaryPart.Position).Magnitude <= 75 then
							if v:FindFirstChild("LensmenTeam") or v:FindFirstChild("BathroomTeam") then
								game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 600, false, false)
							end
						end
					end
					game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(rapidFireProj)
				end
			end)
		end)
		task.wait(2.5)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = false
		task.wait(7.5)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.RapidFire.Value = false
	end
	if input.KeyCode == Enum.KeyCode.T and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false and game.Players.LocalPlayer.Character.Attacks.Speedblitz.Value == false then
		game.Players.LocalPlayer.Character.Attacks.Speedblitz.Value = true
		game.Players.LocalPlayer.Character.Speederson.Value = 700
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.juggernautfast, true)
		game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.PrimaryPart, "tc_13_teleport")
		local hit = false
		game.Players.LocalPlayer.Character["c1a4d_generator_body_reference.smd.001"].CanTouch = true
		jugRam = game.Players.LocalPlayer.Character["c1a4d_generator_body_reference.smd.001"].Touched:Connect(function(toucher)
			if hit == false and not toucher:IsDescendantOf(workspace.Debris) then
				hit = true
				game.Players.LocalPlayer.Character["c1a4d_generator_body_reference.smd.001"].CanTouch = false
				game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.PrimaryPart, "electric_explosion1")
				game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ASTRO.jughitvfx, game.Players.LocalPlayer.Character.PrimaryPart.CFrame, "UTCMLaserExplode")
				game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.juggernautfast, false)
				game.Players.LocalPlayer.Character.Speederson.Value = 200
				task.spawn(function()
					task.wait(10)
					game.Players.LocalPlayer.Character.Attacks.Speedblitz.Value = false
				end)
				for i, v in pairs(workspace:GetChildren()) do
					if v:IsA("Model") and not v:FindFirstChild("AstroTeam") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.PrimaryPart and (game.Players.LocalPlayer.Character.PrimaryPart.Position-v.PrimaryPart.Position).Magnitude <= 120 then
						if v:FindFirstChild("BathroomTeam") or v:FindFirstChild("LensmenTeam") then
							game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 500, false, false)
						end
					end
				end
				jugRam:Disconnect()
			end
		end)
		task.wait(4.5)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and hit == true then return end
		game.Players.LocalPlayer.Character["c1a4d_generator_body_reference.smd.001"].CanTouch = false
		if jugRam then jugRam:Disconnect() end
		game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.juggernautfast, false)
		game.Players.LocalPlayer.Character.Speederson.Value = 200
		task.wait(10)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		game.Players.LocalPlayer.Character.Attacks.Speedblitz.Value = false
	end
	if input.KeyCode == Enum.KeyCode.X and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false then
		if game.Players.LocalPlayer.Character.Attacks.AstroShield.Value == false then
			game.Players.LocalPlayer.Character.Attacks.ShieldPunch.Value = true
			game.Players.LocalPlayer.Character.Attacks.AstroShield.Value = true
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game.Players.LocalPlayer.Character)
			game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.PrimaryPart, "combineshield_activate")
			local shield = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").MorphShow.AstroJuggernaut.Shield, Vector3.new(0, 1000, 0))
			if not workspace.Debris:FindFirstChild("OldJuggyProps") then
				Instance.new("Folder", workspace.Debris).Name = "OldJuggyProps"
			end
			shield.Parent = workspace.Debris.OldJuggyProps
			local bodyvel = Instance.new("BodyVelocity", shield)
			bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
			bodyvel.Velocity = Vector3.new(0, 0, 0)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", shield, 0)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(shield.Attachment, true)
			if not game.Players.LocalPlayer.Character:FindFirstChild("shieldPair") then
				local shieldPair = Instance.new("Part", game.Players.LocalPlayer.Character)
				shieldPair.Massless = true
				shieldPair.Name = "shieldPair"
				shieldPair.Size = Vector3.new(0.1, 0.1, 0.1)
				shieldPair.Transparency = 1
				shieldPair.CanCollide = false
				shieldPair.CFrame = game.Players.LocalPlayer.Character.PrimaryPart.CFrame * CFrame.new(-3, 4, -42) * CFrame.Angles(math.rad(98), math.rad(2), math.rad(85))
				local weldofshpair = Instance.new("WeldConstraint", shieldPair)
				weldofshpair.Part0 = game.Players.LocalPlayer.Character.PrimaryPart
				weldofshpair.Part1 = shieldPair
			end
			task.spawn(function()
				while shield ~= nil do
                    if game.Players.LocalPlayer.Character:FindFirstChild("shieldPair") then
					    shield.CFrame = game.Players.LocalPlayer.Character.shieldPair.CFrame
                    end
					game:GetService("RunService").Heartbeat:Wait()
				end
			end)
			local ShieldAnim = Instance.new("Animation")
			ShieldAnim.AnimationId = "rbxassetid://125154974424516"
			local ShieldAnimpl = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(ShieldAnim)
			ShieldAnimpl.Priority = Enum.AnimationPriority.Action4
			ShieldAnimpl:Stop()
			ShieldAnimpl:Play()
			task.wait(10)
			if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", shield, 1)
			game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(shield.Attachment, false)
			ShieldAnimpl:Stop()
			game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.PrimaryPart, "combineshield_activate")
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, game.Players.LocalPlayer.Character)
			game.Players.LocalPlayer.Character.Attacks.ShieldPunch.Value = false
			task.wait(10)
			if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
			game.Players.LocalPlayer.Character.Attacks.AstroShield.Value = false
		else
			if workspace.Debris.OldJuggyProps:FindFirstChild("Shield") and game.Players.LocalPlayer.Character.Attacks.ShieldPunch.Value == true then
				game.Players.LocalPlayer.PlayerScripts.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Debris.OldJuggyProps.Shield.Shield1, "GManShieldImpact"})
			end
		end
	end
	if input.KeyCode == Enum.KeyCode.G and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false and game.Players.LocalPlayer.Character.Attacks.JuggernautSight.Value == false then
		game.Players.LocalPlayer.Character.Attacks.JuggernautSight.Value = true
		game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character.Head, "JuggernautLine1")
		task.wait(math.random(200, 275)/100)
		task.spawn(function()
			task.wait(15)
			if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
			game.Players.LocalPlayer.Character.Attacks.JuggernautSight.Value = false
		end)
		if not game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") then return end
		for i, v in pairs(workspace:GetChildren()) do
			if v:IsA("Model") and not v:FindFirstChild("ISBUILDING") and not v:FindFirstChild("AstroTeam") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Head") then
				if v:FindFirstChild("BathroomTeam") or v:FindFirstChild("LensmenTeam") then
					game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(v.Head, "SharpshootFindTarget")
					task.spawn(function()
						local xray = Instance.new("Highlight", v)
						xray.OutlineTransparency = 1
						xray.FillTransparency = 0.4
						xrayaway = game:GetService("TweenService"):Create(xray, TweenInfo.new(12), {FillTransparency = 1})
						xrayaway:Play()
						xrayaway.Completed:Wait()
						xray:Destroy()
					end)
					task.wait(math.random(1, 3)/10)
				end
			end
		end
	end
	if input.KeyCode == Enum.KeyCode.Z and gameProcessed == false and game.Players.LocalPlayer.Character:FindFirstChild("Old_XJug") and game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value == false and game.Players.LocalPlayer.Character.Attacks.ShieldPunch.Value == false and game.Players.LocalPlayer.Character.Attacks.Warp.Value == false then
		if not game.Players.LocalPlayer.Character:FindFirstChild("DONDOLF") then
			game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = true
			game.Players.LocalPlayer.Character.Attacks.Warp.Value = true
			game.Players.LocalPlayer.PlayerScripts.DoAttackz:Fire("AstroReworks", "AstroWarpRework", game.Players.LocalPlayer.Name)
			waitForRemove = game.Players.LocalPlayer.Character.ChildRemoved:Connect(function(child)
				if child.Name == "DONDOLF" then
					print("[SCRIPT] Old Juggy Warp Back")
					waitForRemove:Disconnect()
					task.wait(1)
					game.Players.LocalPlayer.Character.Attacks.GlobalCooldown.Value = false
					task.wait(10)
					game.Players.LocalPlayer.Character.Attacks.Warp.Value = false
				end
			end)
		end
	end
end)
else
game.Players.LocalPlayer:FindFirstChild("JugOldAntiWE"):Destroy()
if _G.JugAddedWE then _G.JugAddedWE:Disconnect() end
if _G.ifnewjug then _G.ifnewjug:Disconnect() end
if _G.OldJugControlButtons then _G.OldJugControlButtons:Disconnect() end
if _G.jugDied then _G.jugDied:Disconnect() end
end
end)

bindas:Toggle("Better Necro Control", false, function(bool)
if bool then
_G.ifnewnecro = game.Players.LocalPlayer.CharacterAdded:Connect(function()
if game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" then
Instance.new("Folder", game.Players.LocalPlayer.Character).Name = "Attacks"
if game.Players.LocalPlayer.Character:FindFirstChild("NecromancerControl") then
game.Players.LocalPlayer.Character:FindFirstChild("NecromancerControl").Disabled = true
end
end
end)
_G.NecroscriptM1Inputs = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
	if input.UserInputType == Enum.UserInputType.MouseButton1 and focusedInput == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("Swinging") then
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then return end
		if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("HideScythe") then
			local atk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			atk.Name = "Swinging"
			game:GetService("Debris"):AddItem(atk, 5)
			local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			stopothatk.Name = "cannot"
			game:GetService("Debris"):AddItem(stopothatk, 3)
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("swingScytheAnim") then
				local swingScythe = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
				swingScythe.AnimationId = "rbxassetid://105955283573189"
				swingScythe.Name = "swingScytheAnim"
			end
			local swingScytheplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.swingScytheAnim)
			swingScytheplay:Play()
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "ExobladeBeamSlash"})
			task.wait(1.2)
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "ExobladeBeamSlash"})
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.weapon.vfx.Attachment, "Mumble"..math.random(1,5)})
			for i, v in pairs(workspace:GetChildren()) do
				if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.HumanoidRootPart.Position).Magnitude <= 90 then
					game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 1800, false, false)
				end
			end
			task.wait(1.8)
			if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and not game.Players.LocalPlayer.Character:FindFirstChild("DeadS") then
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
				game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
			end
		else
			local atk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			atk.Name = "Swinging"
			game:GetService("Debris"):AddItem(atk, 1)
			local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			stopothatk.Name = "cannot"
			game:GetService("Debris"):AddItem(stopothatk, 1)
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("swingClawAnim") then
				local swingClaw = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
				swingClaw.AnimationId = "rbxassetid://102704270182527"
				swingClaw.Name = "swingClawAnim"
			end
			local swingClawplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.swingClawAnim)
			swingClawplay:Play(0.2)
			swingClawplay:AdjustSpeed(0.6)
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "FastHandMovement"})
			task.wait(0.15)
			local hitnonai = false
			for i, v in pairs(workspace:GetChildren()) do
				if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.HumanoidRootPart.Position).Magnitude <= 50 then
					hitnonai = true
					game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 1000, false, true)
					game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {v.HumanoidRootPart, "Claw_Hit"..math.random(1,2)})
					game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.UTCM.HitEffectYellow, v.HumanoidRootPart.CFrame)
					break
				end
			end
		end
	end
	if input.KeyCode == Enum.KeyCode.F and focusedInput == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("Throwing") and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("HideScythe") then
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then return end
		local nearestPlayer = GetNearestPlayer(500, game.Players.LocalPlayer.Character.HumanoidRootPart, "LensmenTeam", false)
		if nearestPlayer then
			local atk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			atk.Name = "Throwing"
			game:GetService("Debris"):AddItem(atk, 5)
			local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			stopothatk.Name = "cannot"
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("throwScytheAnim") then
				local throwScythe = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
				throwScythe.AnimationId = "rbxassetid://105134095259661"
				throwScythe.Name = "throwScytheAnim"
			end
			local throwScytheplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.throwScytheAnim)
			throwScytheplay.Name = "ScytheThrowing"
			throwScytheplay:Play()
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "Mumble"..math.random(1,5)})
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", game.Players.LocalPlayer.Character.weapon, 1)
			local ScytheClone = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.MiscProjectiles.Scythe, game.Players.LocalPlayer.Character.weapon.Position)
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {ScytheClone, "CosmicLaserLoop"})
			local BD = Instance.new("BodyVelocity", ScytheClone)
			BD.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
			BD.Velocity = Vector3.new(0, 0, 0)
			diedDuringThrow = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
				if ScytheClone then
					game:GetService("ReplicatedStorage").CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(ScytheClone)
				end
				diedDuringThrow:Disconnect()
			end)
			task.spawn(function()
				local StartPos = game.Players.LocalPlayer.Character.weapon.Position
				local EndPos = nearestPlayer.HumanoidRootPart.Position
				local CalcMagnitude = (StartPos - EndPos).Magnitude
				local CalcDistance = (StartPos - EndPos) / 2
				local DevideMagnitude = CalcMagnitude / 2
				local calcArg3 = CFrame.new((CFrame.new(StartPos)) * (CalcDistance / -1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
				local calcArg4 = CFrame.new((CFrame.new(EndPos)) * (CalcDistance / 1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
				ScytheClone.Massless = true
				local OkBoomer = 10
				for loopMath = 5, CalcMagnitude, 5 do
					if ScytheClone then
						local newMag = loopMath / CalcMagnitude
						ScytheClone.CFrame = ScytheClone.CFrame:Lerp(CFrame.new(lerpModule:cubicBezier(newMag, StartPos, calcArg3, calcArg4, EndPos), EndPos), newMag)
						ScytheClone.Orientation += Vector3.new(0, 0, OkBoomer)
						OkBoomer += 1.5
						if ScytheClone then
							ScytheClone.CFrame = ScytheClone.CFrame 
						end
						game:GetService("RunService").Heartbeat:Wait()
					else
						break
					end
				end
				if ScytheClone and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
					if nearestPlayer and nearestPlayer:FindFirstChildOfClass("Humanoid") and not nearestPlayer:FindFirstChildOfClass("DeadS") then
						game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(nearestPlayer.Humanoid, 3250, false, false)
					end
					local StartPos = ScytheClone.Position
					local EndPos = game.Players.LocalPlayer.Character.weapon.Position
					local CalcMagnitude = (StartPos - EndPos).Magnitude
					local CalcDistance = (StartPos - EndPos) / 2
					local DevideMagnitude = CalcMagnitude / 2
					local calcArg3 = CFrame.new((CFrame.new(StartPos)) * (CalcDistance / -1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
					local calcArg4 = CFrame.new((CFrame.new(EndPos)) * (CalcDistance / 1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
					ScytheClone.Massless = true
					local OkBoomer = 10
					for loopMath = 5, CalcMagnitude, 5 do
						if ScytheClone then
							local newMag = loopMath / CalcMagnitude
							ScytheClone.CFrame = ScytheClone.CFrame:Lerp(CFrame.new(lerpModule:cubicBezier(newMag, StartPos, calcArg3, calcArg4, EndPos), EndPos), newMag)
							ScytheClone.Orientation += Vector3.new(0, 0, OkBoomer)
							OkBoomer += 1.5
							if ScytheClone then
								ScytheClone.CFrame = ScytheClone.CFrame 
							end
							game:GetService("RunService").Heartbeat:Wait()
						else
							break
						end
					end
					if diedDuringThrow then
						diedDuringThrow:Disconnect()
					end
					if ScytheClone then
						game:GetService("Debris"):AddItem(atk, 8)
						game:GetService("Debris"):AddItem(stopothatk, 2)
						game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(ScytheClone, true)
						if ScytheClone:WaitForChild("STARMANBUFF", 5) then
							for _, v in ipairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
								if v.Name == "ScytheThrowing" then
									v:Stop()
									v:Destroy()
								end
							end
							game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(ScytheClone)
							game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", game.Players.LocalPlayer.Character.weapon, 0)
						end
					end
				end
			end)
		else
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("NotifCd") then
				game:GetService("ReplicatedStorage").SFX.bee:Play()
				game:GetService("Players").LocalPlayer.PlayerScripts.Binds.Notifiaction:Fire("Notification!", "Boomerang cannot be used when there are no targets nearby.")
				local insta = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
				insta.Name = "NotifCd"
				game:GetService("Debris"):AddItem(insta, 10)
			end
		end
	end
	if input.KeyCode == Enum.KeyCode.T and focusedInput == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then return end
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoLaser") then
			game.Players.LocalPlayer.Character.Attacks.DoLaser:Destroy()
			for _, v in ipairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
				if v.Name == "PlayingPoint" then
					v:Stop()
					v:Destroy()
				end
			end
			game.ReplicatedStorage.CombatRemotes.LaserRework:FireServer("LaserSound", game.Players.LocalPlayer.Character.HumanoidRootPart, false)
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "laserend"})
			game:GetService("ReplicatedStorage").Remotes.GManLaser:FireServer(game.Players.LocalPlayer.Character, false, "mEGa")
		else
			Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks).Name = "DoLaser"
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("laserPointAnim") then
				local laserPoint = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
				laserPoint.AnimationId = "rbxassetid://111648966507610"
				laserPoint.Name = "laserPointAnim"
			end
			local laserPointplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.laserPointAnim)
			laserPointplay.Name = "PlayingPoint"
			laserPointplay:Play()
			game.ReplicatedStorage.CombatRemotes.LaserRework:FireServer("LaserSound", game.Players.LocalPlayer.Character.HumanoidRootPart, true)
			game:GetService("ReplicatedStorage").Remotes.GManLaser:FireServer(game.Players.LocalPlayer.Character, true, "mEGa")
			local Mouse = game.Players.LocalPlayer:GetMouse()
			task.spawn(function()
				while game.Players.LocalPlayer.Character and not game.Players.LocalPlayer.Character:FindFirstChild("DeadS") and game.Players.LocalPlayer.Character.Attacks:FindFirstChild("DoLaser") do
					if Mouse.Target ~= nil then
						game.ReplicatedStorage.CombatRemotes.LaserRework:FireServer("LaserSnapToPos", game.Players.LocalPlayer.Character.Thing100, Mouse.Hit.Position)
						if math.random(1, 4) == 1 then
							game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.LASEREXPLOSIONS.GREEN, CFrame.new(Mouse.Hit.Position, Vector3.new(0,0,0)), "UTCMLaserExplode")
							for i, v in pairs(workspace:GetChildren()) do
								if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (Mouse.Hit.Position-v.HumanoidRootPart.Position).Magnitude <= 90 then
									local CalcDmg = math.floor(250 - (1.05 * (v.PrimaryPart.Position-game.Players.LocalPlayer.Character.PrimaryPart.Position).Magnitude / 1.8))
									if CalcDmg > 40 then
    									game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, CalcDmg, false, false)
									else
    									game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 40, false, false)
									end
								end
							end
						end
					end
					task.wait(0.02)
				end
			end)
		end
	end
	if input.KeyCode == Enum.KeyCode.R and focusedInput == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("Stomping") then
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then return end
		local atk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
		atk.Name = "Stomping"
		game:GetService("Debris"):AddItem(atk, 10)
		local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
		stopothatk.Name = "cannot"
		game:GetService("Debris"):AddItem(stopothatk, 2)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
		if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("stompingAnim") then
			local stomping = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
			stomping.AnimationId = "rbxassetid://78093450137935"
			stomping.Name = "stompingAnim"
		end
		local stompingplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.stompingAnim)
		stompingplay:Play()
		task.wait(0.6)
		local StompBox = Instance.new("Part", workspace.Debris)
		StompBox.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(1, -58, -50)
		StompBox.Name = "NecroStomp"
		StompBox.Anchored = true
		StompBox.CanCollide = false
		StompBox.Transparency = 1
		game:GetService("Debris"):AddItem(StompBox, 5)
		game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.SOUNDWOMAN.Cloud, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(1, -58, -50), "ExobladeBigHit")
		for i, v in pairs(workspace:GetChildren()) do
			if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (StompBox.Position-v.HumanoidRootPart.Position).Magnitude <= 40 then
				game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 1250, false, false, "Footstep")
			end
		end
		task.wait(1)
		if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and not game.Players.LocalPlayer.Character:FindFirstChild("DeadS") then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
		end
	end
	if input.KeyCode == Enum.KeyCode.X and focusedInput == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("Teleporting") then
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then return end
		local atk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
		atk.Name = "Teleporting"
		game:GetService("Debris"):AddItem(atk, 15)
		local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
		stopothatk.Name = "cannot"
		game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game.Players.LocalPlayer.Character)
		local alreadyVisibleParts = {}
		local halfVisibleParts = {}
		for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v:IsA("BasePart") then
				if v.Transparency == 0 then
					table.insert(alreadyVisibleParts, v)
				end
				if v.Transparency > 0 then
					if not v:FindFirstChild("oldTr") then
						local OldTransparency = Instance.new("StringValue", v)
						OldTransparency.Name = "oldTr"
						OldTransparency.Value = v.Transparency
					end
					table.insert(halfVisibleParts, v)
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", v, 1)
				end
			end
		end
		for i, v in pairs(alreadyVisibleParts) do
			game.ReplicatedStorage.CombatRemotes.ComputerCombat:FireServer("JETPACK:OFF", v)
		end
		task.wait(0.8)
		local tpwait = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.SOUNDWOMAN.Cloud)
		tpwait.Anchored = true
		game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", game.Players.LocalPlayer.Character.HumanoidRootPart, workspace.BathroomSpawns:GetChildren()[math.random(1, 7)].CFrame)
		task.wait(1)
		for i, v in pairs(alreadyVisibleParts) do
			game.ReplicatedStorage.CombatRemotes.ComputerCombat:FireServer("JETPACK:ON", v)
		end
		task.wait(1)
		for i, v in pairs(halfVisibleParts) do
			if v:FindFirstChild("oldTr") then
				game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", v, v:FindFirstChild("oldTr").Value)
			end
		end
		game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, game.Players.LocalPlayer.Character)
		stopothatk:Destroy()
	end
	if input.KeyCode == Enum.KeyCode.G and focusedInput == false and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("Throwing") then
		if game.Players.LocalPlayer.Character.Attacks:FindFirstChild("cannot") then return end
		if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("HideScythe") then
			local atk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			atk.Name = "HideScythe"
			local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			stopothatk.Name = "cannot"
			game:GetService("Debris"):AddItem(stopothatk, 2.5)
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("hideScytheAnim") then
				local hideScythe = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
				hideScythe.AnimationId = "rbxassetid://99541049718681"
				hideScythe.Name = "hideScytheAnim"
			end
			local hideScytheplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.hideScytheAnim)
			hideScytheplay:Play(0.4)
			hideScytheplay:AdjustSpeed(0.3)
			task.wait(0.7)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", game.Players.LocalPlayer.Character.weapon, 1)
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GenericBurst"})
		else
			game.Players.LocalPlayer.Character.Attacks.HideScythe:Destroy()
			local stopothatk = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
			stopothatk.Name = "cannot"
			game:GetService("Debris"):AddItem(stopothatk, 2.5)
			if not game.Players.LocalPlayer.Character.Attacks:FindFirstChild("hideScytheAnim") then
				local hideScythe = Instance.new("Animation", game.Players.LocalPlayer.Character.Attacks)
				hideScythe.AnimationId = "rbxassetid://99541049718681"
				hideScythe.Name = "hideScytheAnim"
			end
			local hideScytheplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.Players.LocalPlayer.Character.Attacks.hideScytheAnim)
			hideScytheplay:Play(0.4)
			hideScytheplay:AdjustSpeed(0.3)
			task.wait(0.7)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", game.Players.LocalPlayer.Character.weapon, 0)
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GenericBurst"})
		end
	end
end)
if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name == "BathroomNecromancerGiant" then
if game.Players.LocalPlayer.Character:FindFirstChild("NecromancerControl") then
game.Players.LocalPlayer.Character:FindFirstChild("NecromancerControl").Disabled = true
end
if not game.Players.LocalPlayer.Character:FindFirstChild("Attacks") then
Instance.new("Folder", game.Players.LocalPlayer.Character).Name = "Attacks"
end
end
else
if _G.NecroscriptM1Inputs then _G.NecroscriptM1Inputs:Disconnect() end
if _G.ifnewnecro then _G.ifnewnecro:Disconnect() end
if diedDuringThrow then diedDuringThrow:Disconnect() end
end
end)

local SCRIPT_ClockGiant = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local changemode = Instance.new("ImageButton")
