if game:GetService("Players"):FindFirstChild(targ) and game:GetService("Players"):FindFirstChild(targ).Character ~= nil and game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("Humanoid") then
game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(game:GetService("Players"):FindFirstChild(targ).Character.Humanoid, tonumber(admg), burn, true)
end
end)

oth:Button("Heal Target", function()
