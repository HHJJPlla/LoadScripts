local seeGodLocal = {}
plr:Toggle("God Mode (TP)", false, function(infhp)
if infhp then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name ~= game.Players.LocalPlayer.Name then
table.insert(seeGodLocal, game.Players.LocalPlayer.Character.ChildAdded:Connect(function(child)
	if child.Name == "wefewfegyerw8gerwrgtiju45ty845" then
		child.Name = "ServerGod"
	end
end))
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game.Players.LocalPlayer.Character)
Instance.new("BoolValue", game.Players.LocalPlayer.Character).Name = "GivenGod"
end
table.insert(seeGodLocal, game.Players.LocalPlayer.CharacterAdded:Connect(function()
	if game.Players.LocalPlayer.Character.Name ~= game.Players.LocalPlayer.Name then
		table.insert(seeGodLocal, game.Players.LocalPlayer.Character.ChildAdded:Connect(function(child)
			if child.Name == "wefewfegyerw8gerwrgtiju45ty845" then
				child.Name = "ServerGod"
			end
		end))
		game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game.Players.LocalPlayer.Character)
		Instance.new("BoolValue", game.Players.LocalPlayer.Character).Name = "GivenGod"
	end
end))
else
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("GivenGod") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, game.Players.LocalPlayer.Character)
if game.Players.LocalPlayer.Character:FindFirstChild("GivenGod") then
game.Players.LocalPlayer.Character:FindFirstChild("GivenGod"):Destroy()
end
end
local funcs
for i, v in pairs(seeGodLocal) do
	v:Disconnect()
	funcs = i
end
table.clear(seeGodLocal)
print("[SCRIPT] Disconnected "..funcs.." functions")
end
end)

plr:Toggle("50% Damage Reduce", false, function(reducedmg)
if reducedmg then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name ~= game.Players.LocalPlayer.Name then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", true, game.Players.LocalPlayer.Character)
onNewAddReduce = game.Players.LocalPlayer.CharacterAdded:Connect(function()
if game.Players.LocalPlayer.Character.Name ~= game.Players.LocalPlayer.Name then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", true, game.Players.LocalPlayer.Character)
end
end)
end
else
if game.Players.LocalPlayer:FindFirstChild("DESTRUCTORDEFENSEEEE") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", false, game.Players.LocalPlayer.Character)
end
if onNewAddReduce then onNewAddReduce:Disconnect() end
end
end)

