game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM:ASTRO", playerChr)
if playerChr:WaitForChild("BathroomTeam", 8) then
if playerChr ~= nil then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", game.Players.LocalPlayer.Character:FindFirstChild("LensmenTeam"), Vector3.new(0,-600,0))
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", game.Players.LocalPlayer.Character:FindFirstChild("AstroTeam"), Vector3.new(0,-600,0))
