function fakebossbuttonsactive(bool, activated)
if bool == true then
dashforward.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
dashforwardstroke.Color = Color3.fromRGB(0, 150, 0)
dashup.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
dashupstroke.Color = Color3.fromRGB(0, 150, 0)
rotateattarget.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
rotateattargetstroke.Color = Color3.fromRGB(0, 150, 0)
else
if activated == "Dash Forward" then
dashforward.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
dashforwardstroke.Color = Color3.fromRGB(225, 225, 0)
else
dashforward.BackgroundColor3 = Color3.fromRGB(170, 170, 170)
dashforwardstroke.Color = Color3.fromRGB(140, 140, 140)
end
if activated == "Dash Up" then
dashup.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
dashupstroke.Color = Color3.fromRGB(225, 225, 0)
else
dashup.BackgroundColor3 = Color3.fromRGB(170, 170, 170)
dashupstroke.Color = Color3.fromRGB(140, 140, 140)
end
if activated == "Rotate at Target" then
rotateattarget.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
rotateattargetstroke.Color = Color3.fromRGB(225, 225, 0)
else
rotateattarget.BackgroundColor3 = Color3.fromRGB(170, 170, 170)
rotateattargetstroke.Color = Color3.fromRGB(140, 140, 140)
end
end
end

bosscd = false
bossbulletscd = false
fakecontrols = false
fakeb:Toggle("Boss Controls", false, function(bool)
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
fakecontrols = bool
SCRIPT_BossControls.Enabled = bool
end
end
end)

fakeb:Toggle("Boss Freeze", false, function(bool)
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(v.HumanoidRootPart, bool)
break
end
end
end)

fakeb:Button("Remove Boss", function()
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(v)
if endbosswithsound == true then
game.ReplicatedStorage.Remotes.sfxname:FireServer("bossDefeated", workspace)
end
fakebossbuttonsactive(true)
SCRIPT_BossControls.Enabled = false
attacktype.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
attacktypestroke.Color = Color3.fromRGB(225, 0, 0)
phasetype.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
phasetypestroke.Color = Color3.fromRGB(0, 150, 0)
phasetype.Image = "http://www.roblox.com/asset/?id=10198213112"
workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
break
end
end
end)

bosslasersbind = false
_G.scriptallinputs = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
if input.KeyCode == Enum.KeyCode.B then
if gren == true then
game.ReplicatedStorage.Remotes.GiantCombat:FireServer("Granada", Vector3.new(0, 0, 0))
end
end

if input.KeyCode == Enum.KeyCode.L then
if partt == true then
if game.Players.LocalPlayer:GetMouse().Target:IsA("Part") or game.Players.LocalPlayer:GetMouse().Target:IsA("MeshPart") then
game.ReplicatedStorage.CombatRemotes.DrillCombat:FireServer("DrillTransparent", game.Players.LocalPlayer:GetMouse().Target, applytransa)
end
end
if partac == true then
if game.Players.LocalPlayer:GetMouse().Target:IsA("Part") or game.Players.LocalPlayer:GetMouse().Target:IsA("MeshPart") then
if game.Players.LocalPlayer:GetMouse().Target.Anchored == true then
game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(game.Players.LocalPlayer:GetMouse().Target, false)
else
game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(game.Players.LocalPlayer:GetMouse().Target, true)
end
end
end
if partmv == true then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer:GetMouse().Target:IsA("Part") or game.Players.LocalPlayer:GetMouse().Target:IsA("MeshPart") then
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", game.Players.LocalPlayer:GetMouse().Target, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
end
end
if partcl == true then
if game.Players.LocalPlayer:GetMouse().Target:IsA("Part") or game.Players.LocalPlayer:GetMouse().Target:IsA("MeshPart") then
game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game.Players.LocalPlayer:GetMouse().Target)
end
end
if partds == true then
if game.Players.LocalPlayer:GetMouse().Target:IsA("Part") or game.Players.LocalPlayer:GetMouse().Target:IsA("MeshPart") then
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(game.Players.LocalPlayer:GetMouse().Target)
end
end
end

if input.KeyCode == Enum.KeyCode.C then
if thrower == true then
    for i,v in pairs(workspace:GetDescendants()) do
        if v:FindFirstChild("UBB_SCRIPT_Highlight") and v:FindFirstChild("HumanoidRootPart") then
            game.ReplicatedStorage.CombatRemotes.ThrowCharacter:FireServer(v.HumanoidRootPart)
            for i = 1, 5 do
                game.ReplicatedStorage.Remotes.sfxname:FireServer("BathroomExplode", game.Players.LocalPlayer.Character.HumanoidRootPart)
            end
        end
    end
end
if cloner == true then
    for i,v in pairs(workspace:GetDescendants()) do
        if v:FindFirstChild("UBB_SCRIPT_Highlight") and v:FindFirstChild("HumanoidRootPart") then
            for i = 1, clonevalue do
                game.ReplicatedStorage.CombatRemotes.ThrowCharacter:FireServer(v.HumanoidRootPart)
            end
            game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(game.Players.LocalPlayer.Character.rigidgrab)
        end
    end
end
end

if input.KeyCode == Enum.KeyCode.P then
if mechab == true then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("plasmaLauncher") then
local args = {
    [1] = applyball,
    [2] = game.Players.LocalPlayer:GetMouse().Hit.p,
    [4] = "plasmaLauncher"
}
game.ReplicatedStorage.Remotes.GiantCombat:FireServer(unpack(args))
end
end
if tcmemit == true then
if tcmcooldown == false and game.Players.LocalPlayer.Character ~= nil and  game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.Name == "GiantLensMan" then
tcmcooldown = true
local tcmcorearea = Instance.new("Part", workspace)
tcmcorearea.Name = "SCRIPT_Core_TCM_Area"
tcmcorearea.Anchored = true
tcmcorearea.CanCollide = false
tcmcorearea.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -10, -40)
tcmcorearea.Size = Vector3.new(1, 1, 1)
tcmcorearea.Transparency = 1
game.ReplicatedStorage.Remotes.GiantCombat:FireServer("UTCMCoreFire", nil, true)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.Core.Core.Attachment, true)
task.wait(0.5)
for g = 1, 30 do
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "GiantLensMan" and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
tcmcorearea.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -10, -40)
for i, v in pairs(game.Players:GetPlayers()) do
    if v.Name ~= game.Players.LocalPlayer.Name and v.Character ~= nil and not v.Character:FindFirstChild("SCRIPT_Already_AffectedBy_CoreFire") and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and (workspace["SCRIPT_Core_TCM_Area"].Position-v.Character.HumanoidRootPart.Position).Magnitude <= 50 then
        if v.Character:FindFirstChild("BathroomTeam") or v.Character:FindFirstChild("AstroTeam") then
            game.ReplicatedStorage.Remotes.raydamage:FireServer(v.Character.Humanoid, 800, true)
            local alreadyaffected = Instance.new("BoolValue", v.Character)
            alreadyaffected.Name = "SCRIPT_Already_AffectedBy_CoreFire"
        end
    end 
end
for i, v in pairs(workspace.AIs:GetChildren()) do
    if not v:FindFirstChild("SCRIPT_Already_AffectedBy_CoreFire") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("BathroomTeam") and (workspace["SCRIPT_Core_TCM_Area"].Position-v.HumanoidRootPart.Position).Magnitude <= 50 then
        game.ReplicatedStorage.Remotes.raydamage:FireServer(v.Humanoid, 800, true)
        local alreadyaffected = Instance.new("BoolValue", v)
        alreadyaffected.Name = "SCRIPT_Already_AffectedBy_CoreFire"
    end 
end
end
task.wait(0.15)
end
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "GiantLensMan" then
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.Core.Core.Attachment, false)
end
workspace["SCRIPT_Core_TCM_Area"]:Destroy()
for i, v in pairs(game.Players:GetPlayers()) do
    if v.Name ~= game.Players.LocalPlayer.Name and v.Character ~= nil and v.Character:FindFirstChild("SCRIPT_Already_AffectedBy_CoreFire") then
        v.Character:FindFirstChild("SCRIPT_Already_AffectedBy_CoreFire"):Destroy()
    end
end
for i, v in pairs(workspace.AIs:GetChildren()) do
    if v:FindFirstChild("SCRIPT_Already_AffectedBy_CoreFire") then
        v:FindFirstChild("SCRIPT_Already_AffectedBy_CoreFire"):Destroy()
    end
end
task.wait(5)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "GiantLensMan" then
game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(game.Players.LocalPlayer.Character, "Move")
end
tcmcooldown = false
end
end
end

if input.KeyCode == Enum.KeyCode.T then
if fakecontrols == true and bosscd == false then
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
bosscd = true
fakebossbuttonsactive(false, "Dash Forward")
local tpto1 = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, -50)
local tpto2 = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, -100)
local tpto3 = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, -150)
local tpto4 = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, -180)
local tpto5 = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, -190)
local tpto6 = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, -195)
game.ReplicatedStorage.Remotes.sfxname:FireServer("chain", v.HumanoidRootPart)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto1)
task.wait(0.1)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto2)
task.wait(0.1)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto3)
task.wait(0.1)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto4)
task.wait(0.05)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto5)
task.wait(0.05)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto6)
task.wait(0.05)
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") and v.HumanoidRootPart:FindFirstChild("chain") then
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(v.HumanoidRootPart.chain)
end
end
fakebossbuttonsactive(true)
bosscd = false
end
end
end
end

if input.KeyCode == Enum.KeyCode.Y then
if fakecontrols == true and bosscd == false then
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
bosscd = true
fakebossbuttonsactive(false, "Dash Up")
local tpto1 = v.HumanoidRootPart.CFrame + Vector3.new(0, 40, 0)
local tpto2 = v.HumanoidRootPart.CFrame + Vector3.new(0, 80, 0)
local tpto3 = v.HumanoidRootPart.CFrame + Vector3.new(0, 120, 0)
local tpto4 = v.HumanoidRootPart.CFrame + Vector3.new(0, 135, 0)
local tpto5 = v.HumanoidRootPart.CFrame + Vector3.new(0, 140, 0)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto1)
task.wait(0.1)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto2)
task.wait(0.1)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto3)
task.wait(0.1)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto4)
task.wait(0.05)
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, tpto5)
task.wait(0.05)
fakebossbuttonsactive(true)
bosscd = false
end
end
end
end

if input.KeyCode == Enum.KeyCode.U then
if fakecontrols == true and bosscd == false then
if bossrotateatmouse == false then
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
local nearestPlayer = GetNearestPlayer()
if nearestPlayer then
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, CFrame.lookAt(v.HumanoidRootPart.Position, nearestPlayer.Character.HumanoidRootPart.Position))
end
end
end
else
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", v.HumanoidRootPart, CFrame.lookAt(v.HumanoidRootPart.Position, game.Players.LocalPlayer:GetMouse().Hit.Position))
end
end
end
end
end

if input.KeyCode == Enum.KeyCode.R then
if fakecontrols == true and phasetypemoment == false then
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
bosslasersbind = not bosslasersbind
if bosslasersbind == true then
game.ReplicatedStorage.Remotes.GManLaser:FireServer(v, true)
attacktype.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
attacktypestroke.Color = Color3.fromRGB(0, 150, 0)
else
game.ReplicatedStorage.Remotes.GManLaser:FireServer(v, false)
attacktype.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
attacktypestroke.Color = Color3.fromRGB(225, 0, 0)
end
end
end
end
if fakecontrols == true and bosscd == false and bossbulletscd == false and phasetypemoment == true then
for i, v in pairs(workspace:GetChildren()) do
if v.Name == "GManBathroom2" and v:FindFirstChild("Grabbed") then
bossbulletscd = true
attacktype.BackgroundColor3 = Color3.fromRGB(170, 170, 170)
attacktypestroke.Color = Color3.fromRGB(140, 140, 140)
local gemen = v
function runfirstbullet()
game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game.ReplicatedStorage.PlasmaBall2, gemen["gun_r.smd.001"].CFrame * CFrame.new(20, -1, -15))
repeat task.wait() until workspace.Debris:FindFirstChild("PlasmaBall2")
local theshootball = workspace.Debris:FindFirstChild("PlasmaBall2")
theshootball.Name = "PlasmaScriptRun"
game.ReplicatedStorage.CombatRemotes.DrillCombat:FireServer("DrillTransparent", theshootball, 0)
game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(gemen.Thing0, "LaserCannon")
game:GetService("ReplicatedStorage").CombatRemotes.SupermanBuff:FireServer(theshootball, true)
repeat task.wait() until theshootball:FindFirstChild("STARMANBUFF")
local movingball = 0
for i = 1, 50 do
movingball = movingball - 2.3
local args = {
    [1] = "cf",
    [2] = theshootball,
    [3] = theshootball.CFrame * CFrame.new(0, 0, movingball)
}
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer(unpack(args))
task.wait(0.01)
end
for i, v in pairs(game.Players:GetPlayers()) do
if v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 and (theshootball.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 35 then
if v.Character:FindFirstChild("LensmenTeam") or v.Character:FindFirstChild("AstroTeam") then
game.ReplicatedStorage.Remotes.raydamage:FireServer(v.Character.Humanoid, 50, false)
end
end
end
local args = {
    [1] = game.ReplicatedStorage.Assets.Specialized.ASTRO.astrotroopexplo,
    [2] = theshootball.CFrame,
    [3] = "233"
}
game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(unpack(args))
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(theshootball)
end

function runsecondbullet()
game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game.ReplicatedStorage.PlasmaBall2, gemen["gun_r.smd.001"].CFrame * CFrame.new(-20, -1, -15))
repeat task.wait() until workspace.Debris:FindFirstChild("PlasmaBall2")
local theshootball = workspace.Debris:FindFirstChild("PlasmaBall2")
theshootball.Name = "PlasmaScriptRun"
game.ReplicatedStorage.CombatRemotes.DrillCombat:FireServer("DrillTransparent", theshootball, 0)
game:GetService("ReplicatedStorage").Remotes.PlaySound:FireServer(gemen.Thing1, "LaserCannon")
game:GetService("ReplicatedStorage").CombatRemotes.SupermanBuff:FireServer(theshootball, true)
repeat task.wait() until theshootball:FindFirstChild("STARMANBUFF")
local movingball = 0
for i = 1, 50 do
movingball = movingball - 2.3
local args = {
    [1] = "cf",
    [2] = theshootball,
    [3] = theshootball.CFrame * CFrame.new(0, 0, movingball)
}
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer(unpack(args))
task.wait(0.01)
end
for i, v in pairs(game.Players:GetPlayers()) do
if v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 and (theshootball.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 50 then
if v.Character:FindFirstChild("LensmenTeam") or v.Character:FindFirstChild("AstroTeam") then
game.ReplicatedStorage.Remotes.raydamage:FireServer(v.Character.Humanoid, 50, false)
end
end
end
local args = {
    [1] = game.ReplicatedStorage.Assets.Specialized.ASTRO.astrotroopexplo,
    [2] = theshootball.CFrame,
    [3] = "233"
}
game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(unpack(args))
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(theshootball)
end

for i = 1, 5 do
coroutine.wrap(function()
    runfirstbullet()
end)()
task.wait(0.15)
coroutine.wrap(function()
    runsecondbullet()
end)()
task.wait(0.15)
end
attacktype.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
attacktypestroke.Color = Color3.fromRGB(0, 150, 0)
bossbulletscd = false
end
end
end
end
end)

library:Keybind("LeftAlt")

print("[SCRIPT] Fully Loaded!")
Notify({
Description = "Press Alt to Hide The Script.";
Duration = 3;
});
