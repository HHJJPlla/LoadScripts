if not game.Players.LocalPlayer.Character:FindFirstChild("GivenGod") then
    game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game.Players.LocalPlayer.Character)
end
task.wait(5)
if GMan5Alive == false then return end
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.ba.dizz, false)
game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.BOSS.dizzyafterward, game.Players.LocalPlayer.Character.Collision.CFrame, "GMan5.0Alarmsound")
if not game.Players.LocalPlayer.Character:FindFirstChild("GivenGod") then
    game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, game.Players.LocalPlayer.Character)
end
dizzyplay:Stop(1)
game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(game.Players.LocalPlayer.Character.HumanoidRootPart, false)
task.wait(1)
if GMan5Alive == false then return end
G2L["7"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0)
G2L["8"]["Color"] = Color3.fromRGB(225, 0, 0)
game.Players.LocalPlayer.Character["GMan5Attacks"]["DoingAtk"].Value = false
task.wait(4)
if GMan5Alive == false then return end
