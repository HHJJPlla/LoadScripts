game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SET:ZE:OLD:HP", game:GetService("Players"):FindFirstChild(targ).Character.Humanoid, game:GetService("Players"):FindFirstChild(targ).Character.Humanoid.Health + admg)
end
end)

oth:Button("God Mode Target", function()
if game:GetService("Players"):FindFirstChild(targ) and game:GetService("Players"):FindFirstChild(targ).Character ~= nil and game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("HumanoidRootPart") then
if not game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("GivenGod") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game:GetService("Players"):FindFirstChild(targ).Character)
local givengod = Instance.new("BoolValue", game:GetService("Players"):FindFirstChild(targ).Character)
givengod.Name = "GivenGod"
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Target has God Mode now.";
Duration = 3;
});
local IconGui = Instance.new("BillboardGui", game:GetService("Players"):FindFirstChild(targ).Character.HumanoidRootPart)
IconGui.Name = "Client_IsGod_Icon"
IconGui.AlwaysOnTop = true
IconGui.Size = UDim2.new(0, 30, 0, 30)
IconGui.MaxDistance = 5000
local Icon = Instance.new("ImageLabel", IconGui)
Icon.Name = "Icon"
Icon.Image = "http://www.roblox.com/asset/?id=12988744896"
Icon.ImageColor3 = Color3.fromRGB(0, 0, 255)
Icon.BackgroundTransparency = 1
Icon.Size = UDim2.new(0, 30, 0, 30)
Icon.ImageTransparency = 0.5
else
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, game:GetService("Players"):FindFirstChild(targ).Character)
if game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("GivenGod") then
game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("GivenGod"):Destroy()
end
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Target is no longer God.";
Duration = 3;
});
if game:GetService("Players"):FindFirstChild(targ).Character.HumanoidRootPart:FindFirstChild("Client_IsGod_Icon") then
game:GetService("Players"):FindFirstChild(targ).Character.HumanoidRootPart:FindFirstChild("Client_IsGod_Icon"):Destroy()
end
end
end
end)

