G2L["1"].Enabled = bool
end)

seeMouses = {}
game.Players.LocalPlayer:GetMouse().TargetFilter = workspace.Effects
ai:Toggle("Track Mouses", false, function(bool)
	if bool then
		for _, v in pairs(game.Players:GetPlayers()) do
			if v ~= game.Players.LocalPlayer then
				local part = Instance.new("Part", workspace.Effects)
				part.Name = v.Name.."MouseDP"
				part.Anchored = true
				part.Size = Vector3.new(1, 1, 1)
				part.Color = Color3.fromRGB(255, 0, 0)
				part.CanCollide = false
				local hgh = Instance.new("Highlight", part)
				hgh.Name = "See"
				hgh.FillTransparency = 1
				hgh.OutlineColor = Color3.fromRGB(225,0,0)
				local billb = Instance.new("BillboardGui", part)
				billb.AlwaysOnTop = true
				billb.Size = UDim2.new(0, 55, 0, 30)
				local textl = Instance.new("TextLabel", billb)
				textl.Text = v.Name.."'s Mouse"
				textl.Size = UDim2.new(0, 55, 0, 30)
				textl.TextColor3 = Color3.fromRGB(255, 0, 0)
				textl.TextScaled = true
				textl.BackgroundTransparency = 1
				textl.Font = Enum.Font.Arimo
				if v.Character ~= nil and v.Character.Archivable == true then
					part.Transparency = 0
					billb.Enabled = true
				else
					part.Transparency = 1
					billb.Enabled = false
				end
				if game:GetService("ReplicatedStorage").PlayerInfo.MousePositions:FindFirstChild(v.Name.."Mouse") then
					local positionMouse = game:GetService("ReplicatedStorage").PlayerInfo.MousePositions:FindFirstChild(v.Name.."Mouse")
					table.insert(seeMouses, positionMouse:GetPropertyChangedSignal("Value"):Connect(function()
						part.Position = positionMouse.Value
					end))
				end
				table.insert(seeMouses, v.CharacterAdded:Connect(function()
					if v.Character.Archivable == true then
						if workspace.Effects:FindFirstChild(v.Name.."MouseDP") and workspace.Effects:FindFirstChild(v.Name.."MouseDP"):FindFirstChild("BillboardGui") then
							workspace.Effects:FindFirstChild(v.Name.."MouseDP").Transparency = 0
							workspace.Effects:FindFirstChild(v.Name.."MouseDP").BillboardGui.Enabled = true
						end
						if v:WaitForChild("Humanoid", 5) then
							table.insert(seeMouses, v.Humanoid.Died:Connect(function()
								if workspace.Effects:FindFirstChild(v.Name.."MouseDP") and workspace.Effects:FindFirstChild(v.Name.."MouseDP"):FindFirstChild("BillboardGui") then
									workspace.Effects:FindFirstChild(v.Name.."MouseDP").Transparency = 1
									workspace.Effects:FindFirstChild(v.Name.."MouseDP").BillboardGui.Enabled = false
								end
							end))
						end
					end
				end))
			end
		end
	else
		local funcs
		for i, v in pairs(seeMouses) do
			v:Disconnect()
			funcs = i
		end
		table.clear(seeMouses)
		print("[SCRIPT] Disconnected "..funcs.." functions")
		for _, v in pairs(workspace.Effects:GetChildren()) do
			v:Destroy()
		end
	end
end)

if not game.Players.LocalPlayer.PlayerGui:FindFirstChild("PlaingPlrsGUI") then
local allplrsgui = Instance.new("ScreenGui", game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"));
allplrsgui["Name"] = [[PlaingPlrsGUI]];
allplrsgui["ZIndexBehavior"] = Enum.ZIndexBehavior.Sibling;
allplrsgui.Enabled = false
allplrsgui.ResetOnSpawn = false
local allplrsscroll = Instance.new("ScrollingFrame", allplrsgui);
allplrsscroll["Active"] = true;
allplrsscroll["BorderSizePixel"] = 0;
allplrsscroll["CanvasSize"] = UDim2.new(0, 0, 0.8, 0);
allplrsscroll["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
allplrsscroll["Size"] = UDim2.new(0, 200, 0, 250);
allplrsscroll["ScrollBarImageColor3"] = Color3.fromRGB(0, 0, 0);
allplrsscroll["Position"] = UDim2.new(0.01969, 0, 0.60048, 0);
allplrsscroll["BorderColor3"] = Color3.fromRGB(0, 0, 0);
allplrsscroll["ScrollBarThickness"] = 1;
allplrsscroll["BackgroundTransparency"] = 0.9;
local childr = Instance.new("NumberValue", allplrsgui)
childr.Name = "Children"
childr.Value = 0
local indent = Instance.new("NumberValue", allplrsgui)
indent.Name = "Indent"
indent.Value = 0.066
end
local seePlayers = {}
function GetPlayers_Add(name)
	if not game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..name) then
	local newplr = Instance.new("Frame", game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame);
	newplr["BorderSizePixel"] = 0;
	newplr["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
	local framePos = 0
	framePos = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Indent.Value * game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value
	newplr["Position"] = UDim2.new(0, 0, framePos, 0);
	newplr["Size"] = UDim2.new(0, 200, 0, 50);
	newplr["BorderColor3"] = Color3.fromRGB(0, 0, 0);
	newplr["Name"] = "plr_"..name;
	newplr["BackgroundTransparency"] = 0.7;
	game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value + 1
	local plricon = Instance.new("ImageLabel", newplr);
	plricon["BorderSizePixel"] = 0;
	plricon["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
	plricon["Image"] = "http://www.roblox.com/asset/?id=117259180607823";
	plricon["Size"] = UDim2.new(0, 25, 0, 25);
	plricon["BorderColor3"] = Color3.fromRGB(0, 0, 0);
	plricon["BackgroundTransparency"] = 1;
	plricon["Name"] = [[plriconinit]];
	plricon["Position"] = UDim2.new(0.025, 0, 0.225, 0);
	local nameinit = Instance.new("TextLabel", newplr);
	nameinit["TextWrapped"] = true;
	nameinit["BorderSizePixel"] = 0;
	nameinit["TextSize"] = 14;
	nameinit["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
	nameinit["FontFace"] = Font.new([[rbxasset://fonts/families/Arial.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
	nameinit["TextColor3"] = Color3.fromRGB(255, 255, 255);
	nameinit["BackgroundTransparency"] = 1;
	nameinit["Size"] = UDim2.new(0, 163, 0, 25);
	nameinit["BorderColor3"] = Color3.fromRGB(0, 0, 0);
	nameinit["Text"] = name;
	nameinit["Name"] = [[nameinit]];
	nameinit["Position"] = UDim2.new(0.185, 0, 0, 0);
	local namecharinit = Instance.new("TextLabel", newplr);
	namecharinit["TextWrapped"] = true;
	namecharinit["BorderSizePixel"] = 0;
	namecharinit["TextSize"] = 12;
	namecharinit["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
	namecharinit["FontFace"] = Font.new([[rbxasset://fonts/families/Arial.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
	namecharinit["TextColor3"] = Color3.fromRGB(255, 255, 255);
	namecharinit["BackgroundTransparency"] = 1;
	namecharinit["Size"] = UDim2.new(0, 163, 0, 25);
	namecharinit["BorderColor3"] = Color3.fromRGB(0, 0, 0);
	namecharinit["Text"] = ""..game.Players[name].Character.Name;
	namecharinit["Name"] = [[charnameinit]];
	namecharinit["Position"] = UDim2.new(0.185, 0, 0.5, 0);
	local teamefe = Instance.new("UIGradient", newplr);
	teamefe["Rotation"] = 180;
	teamefe["Name"] = [[teamefe]];
	if game.Players[name].Team.Name == "The Union" then
		teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(0, 150, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(0, 150, 255))};
	elseif game.Players[name].Team.Name == "Bathrooms" then
		teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 0, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 0, 0))};
	elseif game.Players[name].Team.Name == "Astro Bathrooms" then
		teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 125, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 125, 0))};
	elseif game.Players[name].Team.Name == "No Team" then
		teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(120, 120, 120)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(120, 120, 120))};
	end
	end
end

function GetPlayers_UpdateTeam(name)
	if game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..name) then
		local teamefe = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..name).teamefe
		if game.Players[name].Team.Name == "The Union" then
			teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(0, 150, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(0, 150, 255))};
		elseif game.Players[name].Team.Name == "Bathrooms" then
			teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 0, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 0, 0))};
		elseif game.Players[name].Team.Name == "Astro Bathrooms" then
			teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 125, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 125, 0))};
		elseif game.Players[name].Team.Name == "No Team" then
			teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(180, 180, 180)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(120, 120, 120)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(120, 120, 120))};
		else
			teamefe["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 255, 255))};
		end
	end
end

ai:Toggle("Get Playing Players", false, function(bool)
if bool then
game.StarterGui:SetCoreGuiEnabled("PlayerList",  true)
print("---=== Server Players ===---")
print("")
for i, v in pairs(game.Players:GetPlayers()) do
    if v.Character ~= nil and v.Name ~= v.Character.Name and v:FindFirstChild("leaderstats"):FindFirstChild("Time") then
        print(v.Name.." ("..tostring(v.leaderstats.Time.Value)..") is Playing as "..v.Character.Name.." in "..v.Team.Name.. " Team")
    end
end
print("")
local gui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("PlaingPlrsGUI")
gui.Enabled = true
table.insert(seePlayers, game.Players.PlayerAdded:Connect(function(plr)
	table.insert(seePlayers, plr:GetPropertyChangedSignal("TeamColor"):Connect(function()
		GetPlayers_UpdateTeam(plr.Name)
	end))
	table.insert(seePlayers, plr.CharacterAdded:Connect(function(chr)
		if chr.Name ~= plr.Name then
			task.spawn(function()
			GetPlayers_Add(plr.Name)
			table.insert(seePlayers, chr:GetPropertyChangedSignal("Parent"):Connect(function()
				if game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..plr.Name) then
					game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..plr.Name):Destroy()
					game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value - 1
					for i, v in pairs(game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:GetChildren()) do
						i -= 1
						v.Position = UDim2.new(0, 0, game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Indent.Value * i, 0)
					end
				end
			end))
			end)
		end
	end))
end))
table.insert(seePlayers, game.Players.PlayerRemoving:Connect(function(plr)
	if game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..plr.Name) then
		game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..plr.Name):Destroy()
		game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value - 1
		for i, v in pairs(game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:GetChildren()) do
			i -= 1
			v.Position = UDim2.new(0, 0, game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Indent.Value * i, 0)
		end
	end
end))
for i, v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer then
	table.insert(seePlayers, v:GetPropertyChangedSignal("TeamColor"):Connect(function()
		GetPlayers_UpdateTeam(v.Name)
	end))
	task.spawn(function()
	if v.Character and v.Character.Name ~= v.Name and not v.Character:FindFirstChild("DeadS") then
		GetPlayers_Add(v.Name)
		table.insert(seePlayers, v.Character:GetPropertyChangedSignal("Parent"):Connect(function()
			if game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..v.Name) then
				game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..v.Name):Destroy()
				game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value - 1
				for i, v in pairs(game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:GetChildren()) do
					i -= 1
					v.Position = UDim2.new(0, 0, game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Indent.Value * i, 0)
				end
			end
		end))
	end
	end)
	table.insert(seePlayers, v.CharacterAdded:Connect(function(chr)
		if chr.Name ~= v.Name then
			GetPlayers_Add(v.Name)
			table.insert(seePlayers, chr:GetPropertyChangedSignal("Parent"):Connect(function()
				if game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..v.Name) then
					game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:FindFirstChild("plr_"..v.Name):Destroy()
					game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value = game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value - 1
					for i, v in pairs(game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:GetChildren()) do
						i -= 1
						v.Position = UDim2.new(0, 0, game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Indent.Value * i, 0)
					end
				end
			end))
		end
	end))
end
end
else
local gui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("PlaingPlrsGUI")
gui.Enabled = false
local funcs
for i, v in pairs(game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.ScrollingFrame:GetChildren()) do
	v:Destroy()
	game.Players.LocalPlayer.PlayerGui.PlaingPlrsGUI.Children.Value = 0
end
for i, v in pairs(seePlayers) do
	v:Disconnect()
	funcs = i
end
table.clear(seePlayers)
print("[SCRIPT] Disconnected "..funcs.." functions")
end
end)

ai:Button("Clean Server Trash", function()
local trash = 0
for i,v in pairs(workspace:GetChildren()) do
    if v:IsA("Model") and v:FindFirstChildOfClass("Humanoid") and v.Humanoid.Health == 0 and v:FindFirstChild("Grabbed") then
	trash = trash + 1
        game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", v, Vector3.new(0,-600,0))
    end
end
Notify({
Title = "Fixed";
Description = "Just Cleaned "..trash.." Server Trash.";
Duration = 3;
});
end)

ai:Button("Clean Custom Trash", function()
local trash = 0
if workspace:FindFirstChild("CustomAIs") then
for i, v in pairs(workspace.CustomAIs:GetChildren()) do
trash = trash + 1
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(v)
end
end
Notify({
Title = "Fixed";
