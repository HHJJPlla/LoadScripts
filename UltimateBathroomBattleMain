inflfuelcheck:Disconnect()
end)
else
print("[SCRIPT] Waiting for Lasers...")
ifaddedfolderlaser = game.Players.LocalPlayer.ChildAdded:Connect(function(child)
if child:IsA("Folder") and child.Name == "LASER:VARS" then
ifaddedfolderlaser:Disconnect()
