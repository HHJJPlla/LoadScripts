changemode.Name = "changemode"
changemode.Parent = Frame
changemode.BackgroundColor3 = Color3.fromRGB(0, 128, 255)
changemode.BorderColor3 = Color3.fromRGB(0, 0, 0)
changemode.BorderSizePixel = 0
changemode.Position = UDim2.new(0.0500000007, 0, 0, 0)
changemode.Size = UDim2.new(0, 51, 0, 51)
