------

function UserFromBox(String)
   for i,v in pairs(game:GetService("Players"):GetPlayers()) do
      if (string.sub(string.lower(v.Name),1,string.len(String))) == string.lower(String) then
         return v
      end
   end
end
print("[SCRIPT] Functions Loaded")

oth:Label("Player Editing", Color3.fromRGB(135, 206, 235))

oth:Box("Target", function(text, focuslost)
if focuslost then
if UserFromBox(text) ~= nil then
game:GetService("CoreGui").TurtleUiLib:GetChildren()[2].Header.Window.TextBox.Text = tostring(UserFromBox(text))
targ = tostring(UserFromBox(text))
end
end
end)
targ = game.Players.LocalPlayer.Name
game:GetService("CoreGui").TurtleUiLib:GetChildren()[2].Header.Window.TextBox.Text = game.Players.LocalPlayer.Name

oth:Box("Affector", function(text, focuslost)
if focuslost then
if UserFromBox(text) ~= nil then
game:GetService("CoreGui").TurtleUiLib:GetChildren()[2].Header.Window:GetChildren()[3].Text = tostring(UserFromBox(text))
affecttarg = tostring(UserFromBox(text))
end
end
end)
affecttarg = game.Players.LocalPlayer.Name
game:GetService("CoreGui").TurtleUiLib:GetChildren()[2].Header.Window:GetChildren()[3].Text = game.Players.LocalPlayer.Name

admg = 20
oth:Box("Health", function(text, focuslost)
if focuslost then
if tonumber(text) ~= nil then
admg = text
else
admg = 20
game:GetService("CoreGui").TurtleUiLib:GetChildren()[2].Header.Window:GetChildren()[4].Text = 20
end
end
end)
game:GetService("CoreGui").TurtleUiLib:GetChildren()[2].Header.Window:GetChildren()[4].Text = 20

