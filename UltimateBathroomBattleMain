function GetNearestPlayer(maxdst, chrpart, team, preferboss)
local nearestPlayer, nearestDistance
local maxDistance = maxdst
	for _, v in pairs(workspace:GetChildren()) do
		if v:IsA("Model") and not v:FindFirstChild("ISBUILDING") and v ~= game.Players.LocalPlayer.Character and not v:FindFirstChild("wefewfegyerw8gerwrgtiju45ty845") and v:FindFirstChildOfClass("Humanoid") and v.PrimaryPart and v:FindFirstChild(team) then
			if v:FindFirstChild("Humanoid").Health > 0 then
				local character = v
				local distance = (chrpart.Position - v.PrimaryPart.Position).Magnitude
				if distance > maxDistance or (nearestDistance and distance >= nearestDistance) then
					continue
				end
				nearestDistance = distance
				nearestPlayer = v
			end
		end
	end
    if nearestPlayer ~= nil then
        return nearestPlayer
    end
end

local Camera = workspace.Camera
function FlyMove()
    if game.Players.LocalPlayer.Character.Humanoid.MoveDirection == Vector3.new(0, 0, 0) then
        return game.Players.LocalPlayer.Character.Humanoid.MoveDirection
    end
    local cameraMove = (Camera.CFrame * CFrame.new(CFrame.new(Camera.CFrame.p, Camera.CFrame.p + Vector3.new(Camera.CFrame.lookVector.x, 0, Camera.CFrame.lookVector.z)):VectorToObjectSpace(game.Players.LocalPlayer.Character.Humanoid.MoveDirection))).p - Camera.CFrame.p
    if cameraMove == Vector3.new() then
        return cameraMove
    end
    return cameraMove.unit
end

lerpModule = {
	lerp = function(arg1, arg2, arg3, arg4)
		return arg2 + (arg3 - arg2) * arg4
	end;
}
function lerpModule.cubicBezier(arg1, arg2, arg3, arg4, arg5, arg6)
	local any_lerp_result1 = lerpModule:lerp(arg4, arg5, arg2)
	return lerpModule:lerp(lerpModule:lerp(lerpModule:lerp(arg3, arg4, arg2), any_lerp_result1, arg2), lerpModule:lerp(any_lerp_result1, lerpModule:lerp(arg5, arg6, arg2), arg2), arg2)
end

