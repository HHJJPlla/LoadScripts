teamtype = true
actuallyinastro = false
ai:Slider("Team",1,3,1, function(value)
if value == 1 then
teamtype = true
actuallyinastro = false
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window:GetChildren()[6].Text = "Union Team"
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window:GetChildren()[6].TextColor3 = Color3.fromRGB(33, 84, 185)
elseif value == 2 then
teamtype = false
actuallyinastro = false
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window:GetChildren()[6].Text = "Bathroom Team"
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window:GetChildren()[6].TextColor3 = Color3.fromRGB(255, 0, 0)
elseif value == 3 then
teamtype = false
actuallyinastro = true
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window:GetChildren()[6].Text = "Astro Team"
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window:GetChildren()[6].TextColor3 = Color3.fromRGB(213, 115, 61)
end
end)

ai:Label("Union Team", Color3.fromRGB(33, 84, 185))

local morphto = ai:Dropdown("Morph Type", {}, function(bool)
if bool then
game:GetService("ReplicatedStorage").UIRemotes.KillCam:FireServer("Respawn", bool, teamtype, actuallyinastro, false)
game:GetService("Players").LocalPlayer.PlayerScripts:WaitForChild("Binds", 3)
if game:GetService("Players").LocalPlayer.PlayerScripts:FindFirstChild("Binds") and game.ReplicatedStorage:FindFirstChild("MorphShow") and game.ReplicatedStorage:FindFirstChild("MorphShow"):FindFirstChild(bool) then
if game.Players.LocalPlayer:FindFirstChild("WE") then
for i, v in pairs(game.Players.LocalPlayer.WE:GetChildren()) do
v:Destroy()
end
end
game:GetService("Players").LocalPlayer.PlayerScripts.Binds.hudinit:Fire(game.ReplicatedStorage:FindFirstChild("MorphShow"):FindFirstChild(bool), teamtype, true)
else
warn("[SCRIPT] Could not find hud menu for your character")
end
end
end)

for i, v in pairs(game:GetService("ReplicatedStorage").MorphShow:GetChildren()) do
morphto:Button(v.Name)
end

