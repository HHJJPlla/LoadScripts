if not _G.bindFocus then
_G.bindFocus = game:GetService("UserInputService").TextBoxFocused:Connect(function(textbox)
	if textbox.Name == "TextBox" then
		focusedInput = true
		warn("Textbox Detected")
	end
end)
_G.bindLostFocus = game:GetService("UserInputService").TextBoxFocusReleased:Connect(function(textbox)
	if textbox.Name == "TextBox" then
		focusedInput = false
		warn("Textbox Released")
	end
end)
end

