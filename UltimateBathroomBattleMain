game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", timereverseefe, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(22, 13, -34))
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("pos", timebig, game.Players.LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(0, 55, 0))
game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("pos", particles, game.Players.LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(0, 55, 0))
local bodyvel = Instance.new("BodyVelocity", timereverseefe)
bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
bodyvel.Velocity = Vector3.new(0, 0, 0)
local bodyvel = Instance.new("BodyVelocity", timebig)
bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
bodyvel.Velocity = Vector3.new(0, 0, 0)
local bodyvel = Instance.new("BodyVelocity", particles)
bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
bodyvel.Velocity = Vector3.new(0, 0, 0)
timereverseefe.CanCollide = false
timebig.CanCollide = false
particles.CanCollide = false
game.ReplicatedStorage.CombatRemotes.ComputerCombat:FireServer("EMIT", game.Players.LocalPlayer.Character.Lightning.Lightning, 5)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(particles, true)
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Effects, "Time"})
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GiantScreenManDestroyGlasses"})
for i, v in pairs(game.Players:GetPlayers()) do
if v.Character ~= nil and not v.Character:FindFirstChild("Highlight") and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 300 then
if v.Character:FindFirstChild("BathroomTeam") or v.Character:FindFirstChild("AstroTeam") then
game.ReplicatedStorage.Remotes.GiantCombat:FireServer("AJANPYSAHYS", v.Character)
game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Character.Humanoid, 125, false, true)
end
end
end
local doingFreeze = true
coroutine.wrap(function()
while doingFreeze == true do
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
	timereverseefe.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(22, 13, -34)
	timebig.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(0, 55, 0)
	particles.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(0, 55, 0)
end
	game:GetService("RunService").Heartbeat:Wait()
end
end)()
task.wait(2.5)
doingFreeze = false
timestopdef.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
timestopdefstroke.Color= Color3.fromRGB(225, 0, 0)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(timereverseefe, false)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(timebig, false)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(particles, false)
clockfreezeplay:Stop(0.4)
task.wait(1)
clockCanDoOtherAtk = true
task.wait(4)
timestopdef.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
timestopdefstroke.Color= Color3.fromRGB(0, 150, 0)
clocktimestopcd = false
end
end
end
end)
_G.ifnewclock = game.Players.LocalPlayer.CharacterAdded:Connect(function()
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" then
if game.Players.LocalPlayer.Character:FindFirstChild("ClockControl") then
game.Players.LocalPlayer.Character.ClockControl.Disabled = true
if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed < 60 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 60
end
if not game.Players.LocalPlayer.Character:FindFirstChild("NO_KNOCKBACK") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SIMM", true, game.Players.LocalPlayer.Character)
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", true, game.Players.LocalPlayer.Character)
end
end
if not workspace:FindFirstChild("ClockStuff") then
local clockfolder = Instance.new("Folder", workspace)
clockfolder.Name = "ClockStuff"
end
shurikenorigin = {1, 2, 3, 4}
changemode.Image = "http://www.roblox.com/asset/?id=13754070639"
clockmouseclick.Image = "http://www.roblox.com/asset/?id=13754070639"
clockm1mode = false
if game.Players.LocalPlayer.Character:WaitForChild("Humanoid", 5) then
task.wait(2)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
_G.ifclockdied = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
if workspace:FindFirstChild("ClockStuff") then
for i, v in pairs(workspace.ClockStuff:GetChildren()) do
if v.Name == "SCRIPT_Clock_shuriken" then
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(v)
end
end
end
end)
end
end
end)
end
else
SCRIPT_ClockGiant.Enabled = false
if _G.scriptM1Inputs then _G.scriptM1Inputs:Disconnect() end
if _G.ifnewclock then _G.ifnewclock:Disconnect() end
if _G.ifclockdied then _G.ifclockdied:Disconnect() end
if workspace:FindFirstChild("ClockStuff") then
for i, v in pairs(workspace.ClockStuff:GetChildren()) do
if v.Name == "SCRIPT_Clock_shuriken" then
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(v)
end
end
workspace.ClockStuff:Destroy()
end
end
end)

