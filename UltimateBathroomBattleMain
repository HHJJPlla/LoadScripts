ai:Label("Play Sound", Color3.fromRGB(135, 206, 235))

local sfxtoplay = ai:Dropdown("Enable Sound", {"Boss Spawn", "Boss Approach", "Boss Phase", "Holy Ray", "Kill SFX"}, function(name)
if game.Players.LocalPlayer.Character ~= nil then
if name == "Boss Spawn" then
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Effects, "bossSpawn"})
elseif name == "Boss Approach" then
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Effects, "bossAbouttospawn"})
elseif name == "Boss Phase" then
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Effects, "bossPhase"})
elseif name == "Holy Ray" then
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Effects, "HolyRay"})
elseif name == "Kill SFX" then
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {workspace.Effects, "killsfx"})
end
end
end)

