if game:GetService("Players"):FindFirstChild(targ) and game:GetService("Players"):FindFirstChild(targ).Character ~= nil then
if not game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("DESTRUCTORDEFENSEEEE") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", true, game:GetService("Players"):FindFirstChild(targ).Character)
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Target has Protection now.";
Duration = 3;
});
else
