for i, v in pairs(workspace:GetChildren()) do
	if v:IsA("Model") and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("Humanoid") and v.PrimaryPart and (laserHit-v.PrimaryPart.Position).Magnitude <= 75 then
		local CalcDmg = math.floor(400 - (1.2 * (game.Players.LocalPlayer.Character.PrimaryPart.Position-v.PrimaryPart.Position).Magnitude / 1.5))
        if CalcDmg > 40 then
            game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, CalcDmg, false, true, "Laser")
        else
            game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 40, false, true, "Laser")
        end
	end
end
end
end
game:GetService("RunService").Heartbeat:Wait()
end
