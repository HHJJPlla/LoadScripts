local setInvistoGMan = {}
for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
	if v:IsA("BasePart") and v.Name ~= "Shield" then
		if v.Transparency == 0 then
			table.insert(alreadyVisiblePartsofGMan, v)
		end
		if v.Transparency > 0 then
			if not v:FindFirstChild("oldTr") then
				table.insert(setInvistoGMan, v)
				local OldTransparency = Instance.new("StringValue", v)
				OldTransparency.Name = "oldTR"
				OldTransparency.Value = v.Transparency
			end
		end
	end
end
