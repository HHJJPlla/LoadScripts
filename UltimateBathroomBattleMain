mobexplode = false
bindas:Toggle("MOB Explode Damage", false, function(bool)
mobexplode = bool
if mobexplode == true then
ondierunexplode = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
if mobexplode == true and game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "MissileOverloadBathroom" then
local explodearea = Instance.new("Part", workspace)
explodearea.Name = "SCRIPT_Missile_Self-Explode_Area"
explodearea.Anchored = true
explodearea.CanCollide = false
explodearea.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
explodearea.Size = Vector3.new(1, 1, 1)
explodearea.Transparency = 1
task.wait(2.2)
if game.Players.LocalPlayer.Character.Name == "MissileOverloadBathroom" then
explodearea.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
for i, v in pairs(game.Players:GetPlayers()) do
    if v.Name ~= game.Players.LocalPlayer.Name and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and (workspace["SCRIPT_Missile_Self-Explode_Area"].Position-v.Character.HumanoidRootPart.Position).Magnitude <= 350 then
        if v.Character:FindFirstChild("LensmenTeam") or v.Character:FindFirstChild("AstroTeam") then
            game.ReplicatedStorage.Remotes.raydamage:FireServer(v.Character.Humanoid, 5700, false)
            game.ReplicatedStorage.CombatRemotes.BlindKnife:FireServer(v.Character)
        end
    end
end
for i, v in pairs(workspace.AIs:GetChildren()) do
    if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("LensmenTeam") and (workspace["SCRIPT_Missile_Self-Explode_Area"].Position-v.HumanoidRootPart.Position).Magnitude <= 350 then
        game.ReplicatedStorage.Remotes.raydamage:FireServer(v.Humanoid, 6000, false)
    end
end
end
explodearea:Destroy()
end
end)
else
ondierunexplode:Disconnect()
end
end)

repaireraura = false
bindas:Toggle("Repairer Healing", false, function(bool)
repaireraura = bool
if repaireraura == true then
    while repaireraura == true do
        if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "RepairerBathroom" and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
            for i, v in pairs(game.Players:GetPlayers()) do
                if v.Name ~= game.Players.LocalPlayer.Name and v.Character ~= nil and v.Character.Name ~= "WarheadBathroom" and v.Character.Name ~= "ExplosiveBarrelBathroom" and v.Character.Name ~= "AcidExplosiveBarrelBathroom" and v.Character.Name ~= "SupermanBathroom" and v.Character:FindFirstChild("BathroomTeam") and not v.Character:FindFirstChild("AstroTeam") and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid").Health > 0 and v.Character.Humanoid.Health ~= v.Character.Humanoid.MaxHealth and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 175 then
                    game.ReplicatedStorage.Remotes.sfxname:FireServer("heal", v.Character.HumanoidRootPart)
                    game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SET:ZE:OLD:HP", v.Character.Humanoid, v.Character.Humanoid.Health + 150)
                    game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game.StarterPlayer.StarterPlayerScripts.AbilitiesFramework.healeffecto, v.Character.HumanoidRootPart.CFrame)
                end
            end
        end
        task.wait(2)
    end
end
end)

