local othteam = oth:Dropdown("Give Team Target", {"Union Team", "Bathroom Team" , "Astro Team"}, function(teamgiver)
if teamgiver == "Union Team" then
if game:GetService("Players"):FindFirstChild(targ) and game:GetService("Players"):FindFirstChild(targ).Character ~= nil then
if not game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("LensmenTeam") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM", game:GetService("Players"):FindFirstChild(targ).Character)
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Added Lensmen Team to Target.";
Duration = 3;
});
else
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(game.Players:FindFirstChild(targ).Character:FindFirstChild("LensmenTeam"))
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Revoked Lensmen Team from Target.";
Duration = 3;
});
end
end
elseif teamgiver == "Bathroom Team" then
if game:GetService("Players"):FindFirstChild(targ) and game:GetService("Players"):FindFirstChild(targ).Character ~= nil then
if not game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("BathroomTeam") then
local playerChr = game:GetService("Players"):FindFirstChild(targ).Character
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM:ASTRO", playerChr)
if playerChr:WaitForChild("BathroomTeam", 8) then
if playerChr ~= nil then
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(playerChr:FindFirstChild("LensmenTeam"))
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(playerChr:FindFirstChild("AstroTeam"))
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Added Bathroom Team to Target.";
Duration = 3;
});
end
end
else
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(game.Players:FindFirstChild(targ).Character:FindFirstChild("BathroomTeam"))
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Revoked Bathroom Team from Target.";
Duration = 3;
});
end
end
elseif teamgiver == "Astro Team" then
if game:GetService("Players"):FindFirstChild(targ) and game:GetService("Players"):FindFirstChild(targ).Character ~= nil then
if not game:GetService("Players"):FindFirstChild(targ).Character:FindFirstChild("AstroTeam") then
local playerChr = game:GetService("Players"):FindFirstChild(targ).Character
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM:ASTRO", playerChr)
if playerChr:WaitForChild("AstroTeam", 8) then
if playerChr ~= nil then
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(playerChr:FindFirstChild("LensmenTeam"))
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(playerChr:FindFirstChild("BathroomTeam"))
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
Description = "Added Astro Team to Target.";
Duration = 3;
});
end
end
else
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(game.Players:FindFirstChild(targ).Character:FindFirstChild("AstroTeam"))
Notify({
Title = ""..game:GetService("Players"):FindFirstChild(targ).Name;
