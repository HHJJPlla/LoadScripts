if game.Players.LocalPlayer.Character ~= nil then
game.ReplicatedStorage.CombatRemotes:FindFirstChild("MiscWhatever"):FireServer(game.Players.LocalPlayer.Character, "LaserSound", bool)
end
end)

ai:Label("Optimization", Color3.fromRGB(135, 206, 235))

ai:Button("Clean Trash (CLIENT)", function()
local trash = 0
for i,v in pairs(workspace:GetChildren()) do
if v:IsA("Part") and v.Name == "qweqw" then
trash = trash + 1
v:Destroy()
end
end
Notify({
Description = "Just Cleaned "..trash.." Client Trash.";
Duration = 3;
});
end)

ai:Button("Clean Trash (SERVER)", function()
local trash = 0
for i,v in pairs(workspace.Debris:GetChildren()) do
if v:IsA("Part") or v:IsA("MeshPart") or v:IsA("Model") then
trash = trash + 1
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(v)
end
end
for i,v in pairs(workspace:GetChildren()) do
if v:IsA("Part") and v.CanCollide == false and v.Position == Vector3.new(0,0,0) then
trash = trash + 1
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(v)
end
end
for i,v in pairs(workspace:GetChildren()) do
if v.Name ~= "GManBathroom2" and v:IsA("Model") and v:FindFirstChild("Grabbed") then
trash = trash + 1
game.ReplicatedStorage.CombatRemotes.ProjectileDestroy:FireServer(v)
end
end
Notify({
Description = "Just Cleaned "..trash.." Server Trash.";
Duration = 3;
});
end)

