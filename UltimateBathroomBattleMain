clockm1mode = false
ClockHoverOver = false
changemode.MouseButton1Click:Connect(function()
clockm1mode = not clockm1mode
if clockm1mode then
	changemode.Image = "http://www.roblox.com/asset/?id=12920783196"
	clockmouseclick.Image = "http://www.roblox.com/asset/?id=12920783196"
	clockorbspam.Visible = true
	if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("ClockControl") then
		game.Players.LocalPlayer.Character.ClockControl.Flames:Play()
	end
else
	changemode.Image = "http://www.roblox.com/asset/?id=13754070639"
	clockmouseclick.Image = "http://www.roblox.com/asset/?id=13754070639"
	clockorbspam.Visible = false
	if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("ClockControl") then
		game.Players.LocalPlayer.Character.ClockControl.Flames:Play()
	end
end
end)
changemode.MouseEnter:Connect(function()
ClockHoverOver = true
end)
changemode.MouseLeave:Connect(function()
ClockHoverOver = false
end)

