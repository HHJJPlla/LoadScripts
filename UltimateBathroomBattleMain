						local insta = Instance.new("BoolValue", game.Players.LocalPlayer.Character.Attacks)
						insta.Name = "NotifCd"
						game:GetService("Debris"):AddItem(insta, 10)
					end
				end
			end
		end
	end
end)
_G.onbazookachr = game.Players.LocalPlayer.CharacterAdded:Connect(function()
if not game.Players.LocalPlayer.Character:FindFirstChild("Attacks") then
local folder = Instance.new("Folder", game.Players.LocalPlayer.Character)
folder.Name = "Attacks"
local acidrocketslaunch = Instance.new("Animation", folder)
acidrocketslaunch.AnimationId = "rbxassetid://121031420060256"
acidrocketslaunch.Name = "AcidFire"
Instance.new("BoolValue", folder).Name = "RocketCD"
Instance.new("BoolValue", folder).Name = "DoingShield"
Instance.new("BoolValue", folder).Name = "DoSprint"
end
if not workspace:FindFirstChild("ToServerSound") then
local toserversd = Instance.new("Folder", workspace)
toserversd.Name = "ToServerSound"
game.Players.LocalPlayer:GetMouse().TargetFilter = toserversd
end
if game.Players.LocalPlayer.Character:WaitForChild("BazookaGiantControl", 5) then
game.Players.LocalPlayer.Character.BazookaGiantControl.Disabled = true
end
end)
else
G2L["1"].Enabled = false
if _G.BazookaControlButtons then _G.BazookaControlButtons:Disconnect() end
if _G.onbazookachr then _G.onbazookachr:Disconnect() end
end
end)

mobexplode = false
bindas:Toggle("MOB Explode Damage", false, function(bool)
mobexplode = bool
if mobexplode == true then
ondierunexplode = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
if mobexplode == true and game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "MissileOverloadBathroom" then
local explodearea = Instance.new("Part", workspace)
explodearea.Name = "SCRIPT_Missile_Self-Explode_Area"
explodearea.Anchored = true
explodearea.CanCollide = false
explodearea.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
explodearea.Size = Vector3.new(1, 1, 1)
explodearea.Transparency = 1
task.wait(2.2)
if game.Players.LocalPlayer.Character.Name == "MissileOverloadBathroom" then
explodearea.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
for i, v in pairs(workspace:GetChildren()) do
    if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v.PrimaryPart and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("Humanoid") and (workspace["SCRIPT_Missile_Self-Explode_Area"].Position-v.PrimaryPart.Position).Magnitude <= 420 then
        game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 5700, false, true)
        game.ReplicatedStorage.CombatRemotes.BlindKnife:FireServer(v)
		print("[SCRIPT] Bomb Hit called "..v.Name)
    end
end
end
explodearea:Destroy()
end
end)
else
ondierunexplode:Disconnect()
end
end)

repaireraura = false
bindas:Toggle("Repairer Healing", false, function(bool)
repaireraura = bool
if repaireraura == true and game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.Name == "RepairerBathroom" and not game.Players.LocalPlayer.Character:FindFirstChild("SCRIPT_Healing_Around") then
local healingpart = Instance.new("Part", game.Players.LocalPlayer.Character)
healingpart.Name = "SCRIPT_Healing_Around"
healingpart.CanCollide = false
healingpart.Transparency = 0.9
healingpart.Massless = true
healingpart.Size = Vector3.new(130, 75, 130)
healingpart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
local weldofheal = Instance.new("WeldConstraint", healingpart)
weldofheal.Part0 = game.Players.LocalPlayer.Character.HumanoidRootPart
weldofheal.Part1 = healingpart
_G.funnytouch = healingpart.Touched:Connect(function(hit)
	if game:GetService("Players"):GetPlayerFromCharacter(hit.Parent) ~= nil and hit.Parent:FindFirstChild("Humanoid") then
        local player = game:GetService("Players"):GetPlayerFromCharacter(hit.Parent)
        if not player.Character:FindFirstChild("SCRIPT_Was_Healed") and player.Character:FindFirstChild("BathroomTeam") and not player.Character:FindFirstChild("AstroTeam") and player.Character.Name ~= "WarheadBathroom" and player.Character.Name ~= "ExplosiveBarrelBathroom" and player.Character.Name ~= "AcidExplosiveBarrelBathroom" and player.Character.Name ~= "SupermanBathroom" and player.Character:FindFirstChild("Humanoid") and player.Character:FindFirstChild("Humanoid").Health > 0 and player.Character.Humanoid.Health and player.Character.Humanoid.Health ~= player.Character.Humanoid.MaxHealth then
            local washeal = Instance.new("BoolValue", player.Character)
            washeal.Name = "SCRIPT_Was_Healed"
            local oldhp = player.Character.Humanoid.Health
            local healingvalue = {375, 450, 560, 750, 1000}
            game.ReplicatedStorage.CombatRemotes.Healer:FireServer(player.Character, healingvalue[math.random(1, 5)])
            task.wait(3)
            if player.Character ~= nil and player.Character:FindFirstChild("SCRIPT_Was_Healed") then
                player.Character:FindFirstChild("SCRIPT_Was_Healed"):Destroy()
            end
        end
	end
end)
ondieremovehealerpart = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
if healingpart then
_G.funnytouch:Disconnect()
healingpart:Destroy()
end
end)
onchraddhealerpart = game.Players.LocalPlayer.CharacterAdded:Connect(function()
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.Name == "RepairerBathroom" and not game.Players.LocalPlayer.Character:FindFirstChild("SCRIPT_Healing_Around") then
local healingpart = Instance.new("Part", game.Players.LocalPlayer.Character)
healingpart.Name = "SCRIPT_Healing_Around"
healingpart.CanCollide = false
healingpart.Transparency = 0.9
healingpart.Massless = true
healingpart.Size = Vector3.new(130, 75, 130)
healingpart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
local weldofheal = Instance.new("WeldConstraint", healingpart)
weldofheal.Part0 = game.Players.LocalPlayer.Character.HumanoidRootPart
weldofheal.Part1 = healingpart
_G.funnytouch = healingpart.Touched:Connect(function(hit)
	if game:GetService("Players"):GetPlayerFromCharacter(hit.Parent) ~= nil and hit.Parent:FindFirstChild("Humanoid") then
        local player = game:GetService("Players"):GetPlayerFromCharacter(hit.Parent)
        if not player.Character:FindFirstChild("SCRIPT_Was_Healed") and player.Character:FindFirstChild("BathroomTeam") and not player.Character:FindFirstChild("AstroTeam") and player.Character.Name ~= "WarheadBathroom" and player.Character.Name ~= "ExplosiveBarrelBathroom" and player.Character.Name ~= "AcidExplosiveBarrelBathroom" and player.Character.Name ~= "SupermanBathroom" and player.Character:FindFirstChild("Humanoid") and player.Character:FindFirstChild("Humanoid").Health > 0 and player.Character.Humanoid.Health and player.Character.Humanoid.Health ~= player.Character.Humanoid.MaxHealth then
            local washeal = Instance.new("BoolValue", player.Character)
            washeal.Name = "SCRIPT_Was_Healed"
            local oldhp = player.Character.Humanoid.Health
            local healingvalue = {375, 450, 550, 750, 1000}
            game.ReplicatedStorage.CombatRemotes.Healer:FireServer(player.Character, healingvalue[math.random(1, 5)])
            task.wait(3)
            if player.Character ~= nil and player.Character:FindFirstChild("SCRIPT_Was_Healed") then
                player.Character:FindFirstChild("SCRIPT_Was_Healed"):Destroy()
            end
        end
	end
end)
ondieremovehealerpart = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
if healingpart then
_G.funnytouch:Disconnect()
healingpart:Destroy()
end
end)
end
end)
else
if ondieremovehealerpart then ondieremovehealerpart:Disconnect() onchraddhealerpart:Disconnect() end
if _G.funnytouch then _G.funnytouch:Disconnect() end
if game.Players.LocalPlayer.Character:FindFirstChild("SCRIPT_Healing_Around") then
game.Players.LocalPlayer.Character:FindFirstChild("SCRIPT_Healing_Around"):Destroy()
end
end
end)

bindas:Label("Building", Color3.fromRGB(135, 206, 235))

applytransa = 2
bindas:Slider("Transparency",0,1,1, function(value)
if value == 1 then
applytransa = 1
game:GetService("CoreGui").TurtleUiLib:GetChildren()[4].Header.Window:GetChildren()[15].Text = "Invisible"
else
applytransa = 0
game:GetService("CoreGui").TurtleUiLib:GetChildren()[4].Header.Window:GetChildren()[15].Text = "Visible"
end
end)

bindas:Label("Invisible", Color3.fromRGB(255, 255, 255))

