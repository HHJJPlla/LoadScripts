								game:GetService("RunService").Heartbeat:Wait()
							else
								break
							end
						end
						game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RocketClone, true)
						RocketClone:WaitForChild("STARMANBUFF", 5)
						if RocketClone then
							game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.ROCKETS.EXPLODE, RocketClone.CFrame)
							if soundpart then
								game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {soundpart, "PlagueBoom"..math.random(1, 4)})
							end
							for i, v in pairs(workspace:GetChildren()) do
								if v:IsA("Model") and v ~= game.Players.LocalPlayer.Character and v:FindFirstChild("LensmenTeam") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (RocketClone.Position-v.HumanoidRootPart.Position).Magnitude <= 110 then
									game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Humanoid, 2000 - math.random(0, 1000), false, false, "Rocket")
								end
							end
							game:GetService("ReplicatedStorage").CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(RocketClone)
						end
					end)
					task.spawn(function()
						local PlayerPos = game.Players.LocalPlayer.Character["w_rpg_reference.smd.001"].Attachment.WorldPosition
						local RocketClone = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").acidrocket, PlayerPos)
						local bodyvel = Instance.new("BodyVelocity", RocketClone)
						bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
						bodyvel.Velocity = Vector3.new(0, 0, 0)
						local MouseCFrame = nearestEnt.HumanoidRootPart.Position
