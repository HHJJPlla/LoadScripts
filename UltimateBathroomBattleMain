game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window.TextBox.Text = tostring(UserFromBox(text))
spawnattarg = tostring(UserFromBox(text))
end
end
end)
spawnattarg = game.Players.LocalPlayer.Name
game:GetService("CoreGui").TurtleUiLib:GetChildren()[3].Header.Window.TextBox.Text = game.Players.LocalPlayer.Name

lensamount = 1
ai:Slider("Lens Amount",1,10,1, function(value)
lensamount = value
end)

ai:Button("Summon Lens", function()
if game:GetService("Players"):FindFirstChild(spawnattarg) and game:GetService("Players"):FindFirstChild(spawnattarg).Character ~= nil and game:GetService("Players"):FindFirstChild(spawnattarg).Character:FindFirstChild("HumanoidRootPart") then
for i = 1, lensamount do
game.ReplicatedStorage.Remotes.GiantCombat:FireServer("LensheliSpawnAction", game:GetService("Players"):FindFirstChild(spawnattarg).Character.HumanoidRootPart.CFrame, game:GetService("Players"):FindFirstChild(spawnattarg).Character.HumanoidRootPart.CFrame, false)
end
end
end)

