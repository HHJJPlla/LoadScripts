clocktimestopcd = false
clocktpcd = false
isdoingclocktp = false
clockshieldcd = false
clockshielddoing = false
bindas:Toggle("Clock Control", false, function(bool)
clockcontrol = bool
if clockcontrol == true then
SCRIPT_ClockGiant.Enabled = true
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" then
if game.Players.LocalPlayer.Character:FindFirstChild("ClockControl") then
game.Players.LocalPlayer.Character.ClockControl.Disabled = true
end
if not workspace:FindFirstChild("ClockStuff") then
local clockfolder = Instance.new("Folder", workspace)
clockfolder.Name = "ClockStuff"
end
if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed < 60 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 60
end
if not game.Players.LocalPlayer.Character:FindFirstChild("NO_KNOCKBACK") then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SIMM", true, game.Players.LocalPlayer.Character)
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("DIMM", true, game.Players.LocalPlayer.Character)
end
if game.Players.LocalPlayer.Character:WaitForChild("Humanoid", 5) then
_G.ifclockdied = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
if workspace:FindFirstChild("ClockStuff") then
for i, v in pairs(workspace.ClockStuff:GetChildren()) do
if v.Name == "SCRIPT_Clock_shuriken" then
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(v)
end
end
end
end)
end
_G.scriptM1Inputs = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
if input.UserInputType == Enum.UserInputType.MouseButton1 and focusedInput == false then
if clockshielddoing == false and ClockHoverOver == false and clockm1mode == false and clockCanDoOtherAtk and clockpunchcd == false then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and game.Players.LocalPlayer.Character.steampunkrifle["steampunkrifle.mdl/STEAMPUNKRIFLE.smd"].Transparency == 0 then
clockpunchcd = true
clockCanDoOtherAtk = false
clocktpcd = true
clockmouseclick.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
clockmouseclickstroke.Color = Color3.fromRGB(225, 225, 0)
local oldspeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
local clockpunchrandanim = {"140191825261267", "118601228593870"}
local clockpunch = Instance.new("Animation")
clockpunch.AnimationId = "rbxassetid://"..clockpunchrandanim[math.random(1, #clockpunchrandanim)]
local clockpunchplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(clockpunch)
clockpunchplay:Play()
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "ExobladeBeamSlash"})
for i, v in pairs(game.Players:GetPlayers()) do
	if v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 75 then
		if v.Character:FindFirstChild("BathroomTeam") or v.Character:FindFirstChild("AstroTeam") then
			game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Character.Humanoid, 1000, false, false)
		end
	end
end
task.wait(1)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = oldspeed
end
clockCanDoOtherAtk = true
clocktpcd = false
clockmouseclick.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
clockmouseclickstroke.Color = Color3.fromRGB(225, 0, 0)
task.wait(0.7)
clockmouseclick.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
clockmouseclickstroke.Color = Color3.fromRGB(0, 150, 0)
clockpunchcd = false
end
elseif clockshielddoing == false and ClockHoverOver == false and clockm1mode and clockCanDoOtherAtk and clockpunchcd == false then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 and game.Players.LocalPlayer.Character.steampunkrifle["steampunkrifle.mdl/STEAMPUNKRIFLE.smd"].Transparency == 0 then
if game.Players.LocalPlayer:GetMouse().Target ~= nil then
clockpunchcd = true
clockCanDoOtherAtk = false
clocktpcd = true
clockmouseclick.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
clockmouseclickstroke.Color = Color3.fromRGB(225, 225, 0)
local oldspeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
local clockgun = Instance.new("Animation")
clockgun.AnimationId = "rbxassetid://113448272446250"
local clockgunplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(clockgun)
clockgunplay:Play()
coroutine.wrap(function()
local clockproj = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.CLOCK.projectile, game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 5000, 0))
local bodyvel = Instance.new("BodyVelocity", clockproj)
bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
bodyvel.Velocity = Vector3.new(0, 0, 0)
task.wait(0.65)
clockmouseclick.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
clockmouseclickstroke.Color = Color3.fromRGB(225, 0, 0)
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.steampunkrifle["steampunkrifle.mdl/STEAMPUNKRIFLE.smd"].shot1, "MegaLaser"})
if clockproj.Parent ~= nil then
local PlayerPos = game.Players.LocalPlayer.Character.steampunkrifle["steampunkrifle.mdl/STEAMPUNKRIFLE.smd"].shot1.Position
local MouseCFrame = game.Players.LocalPlayer:GetMouse().Hit.Position
local CalcMagnitude = (PlayerPos - MouseCFrame).Magnitude
local CalcDistance = (PlayerPos - MouseCFrame) / 2
local DevideMagnitude = CalcMagnitude / 2
local calcArg3 = CFrame.new((CFrame.new(PlayerPos)) * (CalcDistance / -1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
local calcArg4 = CFrame.new((CFrame.new(MouseCFrame)) * (CalcDistance / 1.5)).Position + Vector3.new(math.random(-DevideMagnitude, DevideMagnitude), math.random(5, 15), math.random(-DevideMagnitude, DevideMagnitude))
clockproj.Position = PlayerPos
clockproj.Anchored = false
clockproj.CanCollide = false
clockproj.Massless = true
for loopMath = 5, CalcMagnitude, 5 do
    if clockproj then
        local newMag = loopMath / CalcMagnitude
	    clockproj.CFrame = clockproj.CFrame:Lerp(CFrame.new(lerpModule:cubicBezier(newMag, PlayerPos, calcArg3, calcArg4, MouseCFrame), MouseCFrame), newMag)
        game:GetService("RunService").Heartbeat:Wait()
    else
        break
    end
end
end
game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(clockproj, true)
if clockproj:WaitForChild("STARMANBUFF", 5) then
game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.CLOCK.bombexplode, clockproj.CFrame, "BigRailgunShoot")
for i, v in pairs(game.Players:GetPlayers()) do
    if v ~= game.Players.LocalPlayer.Name and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and (clockproj.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 120 then
        if v.Character:FindFirstChild("BathroomTeam") or v.Character:FindFirstChild("AstroTeam") then
            game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Character.Humanoid, 500, false, true)
			game.ReplicatedStorage.Remotes.GiantCombat:FireServer("AJANPYSAHYS", v.Character)
        end
    end
end
game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(clockproj)
end
end)()
task.wait(2)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = oldspeed
end
clockmouseclick.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
clockmouseclickstroke.Color = Color3.fromRGB(0, 150, 0)
clockCanDoOtherAtk = true
clocktpcd = false
clockpunchcd = false
end
end
end
end
if input.KeyCode == Enum.KeyCode.R and focusedInput == false then
if clockshielddoing == false and clockCanDoOtherAtk and clockstompcd == false then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
clockstompcd = true
clockCanDoOtherAtk = false
clocktpcd = true
clockstomp.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
clockstompstroke.Color = Color3.fromRGB(225, 225, 0)
local oldspeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
local clockstompa = Instance.new("Animation")
clockstompa.AnimationId = "rbxassetid://131088338136067"
local clockstomapplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(clockstompa)
clockstomapplay:Play()
task.wait(0.6)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
local StompBox = Instance.new("Part", workspace.Debris)
StompBox.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(1, -58, -50)
StompBox.Name = "ClockStomp"
StompBox.Anchored = true
StompBox.Transparency = 1
game:GetService("ReplicatedStorage").Remotes.Stomp:FireServer(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(1, -58, -50))
for i, v in pairs(game.Players:GetPlayers()) do
    if v ~= game.Players.LocalPlayer.Name and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and (StompBox.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 40 then
        if v.Character:FindFirstChild("BathroomTeam") or v.Character:FindFirstChild("AstroTeam") then
            game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Character.Humanoid, 1000, false, true, "Footstep")
        end
    end
end
StompBox:Destroy()
end
task.wait(1)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = oldspeed
end
clockstomp.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
clockstompstroke.Color = Color3.fromRGB(225, 0, 0)
task.wait(1)
clockCanDoOtherAtk = true
clocktpcd = false
task.wait(5)
clockstomp.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
clockstompstroke.Color = Color3.fromRGB(0, 150, 0)
clockstompcd = false
end
end
end
if input.KeyCode == Enum.KeyCode.J and focusedInput == false then
if clockshurikencd == false then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
if shurikenorigin[1] ~= nil then
clockshurikencd = true
local clockshuriken = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game.ReplicatedStorage.Assets.Specialized.CLOCK.Shuriken, game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
clockshuriken.Name = "SCRIPT_Clock_shuriken"
clockshuriken.Parent = workspace.ClockStuff
local shurikenlocalorigin = shurikenorigin[1]
table.remove(shurikenorigin, 1)
local bodyvel = Instance.new("BodyVelocity", clockshuriken)
bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
bodyvel.Velocity = Vector3.new(0, 0, 0)
clockshuriken.CanCollide = false
local shrattacking = Instance.new("BoolValue", clockshuriken)
shrattacking.Name = "isAttacking"
coroutine.wrap(function()
while clockshuriken.Parent ~= nil do
clockshuriken.Orientation += Vector3.new(0, 0, 10)
game:GetService("RunService").Heartbeat:Wait()
end
table.insert(shurikenorigin, shurikenlocalorigin)
end)()
coroutine.wrap(function()
while clockshuriken.Parent ~= nil do
if shrattacking.Value == false then
clockshuriken.Position = game.Players.LocalPlayer.Character.HeadParts["clocklman_gold7.mdl/1.smd.006"]:FindFirstChild("pos"..shurikenlocalorigin).WorldPosition
end
game:GetService("RunService").Heartbeat:Wait()
end
end)()
coroutine.wrap(function()
while clockshuriken.Parent ~= nil do
local nearestPlayer = GetNearestPlayer(425, clockshuriken, "BathroomTeam", true)
if nearestPlayer ~= nil then
	shrattacking.Value = true
	local shurikentween = game:GetService("TweenService"):Create(clockshuriken, TweenInfo.new((game.Players.LocalPlayer.Character.HumanoidRootPart.Position-nearestPlayer.HumanoidRootPart.Position).Magnitude / 500, Enum.EasingStyle.Sine), {Position = nearestPlayer.HumanoidRootPart.Position})
	shurikentween:Play()
	shurikentween.Completed:wait()
	if clockshuriken.Parent ~= nil then
	game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(clockshuriken, true)
	if clockshuriken:WaitForChild("STARMANBUFF", 5) then
	game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(clockshuriken, false)
	if nearestPlayer:FindFirstChild("Humanoid") then
	game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(nearestPlayer.Humanoid, 500, false, true, "Tornado")
	end
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
	local shurikentweenback = game:GetService("TweenService"):Create(clockshuriken, TweenInfo.new(0.5, Enum.EasingStyle.Sine), {Position = game.Players.LocalPlayer.Character.HeadParts["clocklman_gold7.mdl/1.smd.006"]:FindFirstChild("pos"..shurikenlocalorigin).WorldPosition})
	shurikentweenback:Play()
	shurikentweenback.Completed:wait()
	end
	shrattacking.Value = false
end
end
end
task.wait(1.2)
end
end)()
task.wait(0.5)
clockshurikencd = false
end
end
end
end
if input.KeyCode == Enum.KeyCode.F and gameProcessed == false then
if clockshielddoing == false and clockCanDoOtherAtk and megaclockcd == false then
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
megaclockcd = true
clockCanDoOtherAtk = false
clocktpcd = true
megaclock.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
megaclockstroke.Color = Color3.fromRGB(225, 225, 0)
local clockmegaa = Instance.new("Animation")
clockmegaa.AnimationId = "rbxassetid://139748126418392"
local clockmegaaplay = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(clockmegaa)
clockmegaaplay:Play(0.1)
clockmegaaplay:AdjustSpeed(0.5)
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "MegaClock"})
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.Lightning, true)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.TorsoParts.ding, true)
for i, v in pairs(game.Players:GetPlayers()) do
    if v ~= game.Players.LocalPlayer.Name and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.Character.HumanoidRootPart.Position).Magnitude <= 300 then
        if v.Character:FindFirstChild("BathroomTeam") or v.Character:FindFirstChild("AstroTeam") then
            game.ReplicatedStorage.Remotes.AS2E223T234T6U:FireServer(v.Character.Humanoid, 1500, false, true)
        end
    end
end
task.wait(3)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.Lightning, false)
clockmegaaplay:Stop(0.3)
end
megaclock.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
megaclockstroke.Color = Color3.fromRGB(225, 0, 0)
clockCanDoOtherAtk = true
clocktpcd = false
task.wait(2)
if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.Name == "ClockGiant" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.TorsoParts.ding, false)
end
task.wait(3)
