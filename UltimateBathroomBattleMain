G2L["7"].MouseButton1Click:Connect(function()
G2L["7"].Interactable = false
G2L["7"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 0)
G2L["8"]["Color"] = Color3.fromRGB(225, 225, 0)
