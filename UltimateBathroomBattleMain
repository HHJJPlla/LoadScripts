end)
gmanejecttween.Completed:Wait()
DoingMovement = false
MovingTo:Destroy()
if GMan5Alive == false then return end
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GMan5.0EjectStop"})
game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value = false
task.wait(2.5)
if GMan5Alive == false then return end
game.Players.LocalPlayer.Character["GMan5Attacks"].Eject.Value = false
end
if input.KeyCode == Enum.KeyCode.Q and gameProcessed == false and game.Players.LocalPlayer.Character["GMan5Attacks"]["Invisible_Status"].Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].Shield.Value == false and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
game.Players.LocalPlayer.Character["GMan5Attacks"].Shield.Value = true
game.Players.LocalPlayer.Character["GMan5Attacks"]["Shield_Status"].Value = not game.Players.LocalPlayer.Character["GMan5Attacks"]["Shield_Status"].Value
if game.Players.LocalPlayer.Character["GMan5Attacks"]["Shield_Status"].Value == true then
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", game.Players.LocalPlayer.Character.Shield, 0.8)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.Shield, true)
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GManShieldActive"})
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("JIMMY_sHELD", true, game.Players.LocalPlayer.Character)
else
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", game.Players.LocalPlayer.Character.Shield, 1)
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(game.Players.LocalPlayer.Character.Shield, false)
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GManShieldOFF"})
game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("JIMMY_sHELD", false, game.Players.LocalPlayer.Character)
end
task.wait(3)
if GMan5Alive == false then return end
game.Players.LocalPlayer.Character["GMan5Attacks"].Shield.Value = false
end
if input.KeyCode == Enum.KeyCode.Y and gameProcessed == false and game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].Invisible.Value == false and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
game.Players.LocalPlayer.Character["GMan5Attacks"].Invisible.Value = true
game.Players.LocalPlayer.Character["GMan5Attacks"]["Invisible_Status"].Value = not game.Players.LocalPlayer.Character["GMan5Attacks"]["Invisible_Status"].Value
if game.Players.LocalPlayer.Character["GMan5Attacks"]["Invisible_Status"].Value == true then
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "GManShieldActive"})
if not game.Players.LocalPlayer.Character:FindFirstChild("GivenGod") then
    game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, game.Players.LocalPlayer.Character)
end
for i, v in pairs(alreadyVisiblePartsofGMan) do
	game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", v, 0.8)
end
for i, v in pairs(setInvistoGMan) do
	if v:FindFirstChild("oldTR") then
		game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", v, 0.8)
	end
end
else
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "ComputerGiantHighEnergyShoot"})
if not game.Players.LocalPlayer.Character:FindFirstChild("GivenGod") then
    game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, game.Players.LocalPlayer.Character)
end
for i, v in pairs(alreadyVisiblePartsofGMan) do
	game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", v, 0)
end
for i, v in pairs(setInvistoGMan) do
	if v:FindFirstChild("oldTR") then
		game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", v, v:FindFirstChild("oldTR").Value)
	end
end
end
task.wait(5)
if GMan5Alive == false then return end
game.Players.LocalPlayer.Character["GMan5Attacks"].Invisible.Value = false
end
if input.KeyCode == Enum.KeyCode.J and gameProcessed == false and game.Players.LocalPlayer.Character["GMan5Attacks"]["Invisible_Status"].Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value == false and game.Players.LocalPlayer.Character["GMan5Attacks"].Escape.Value == false and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
game.Players.LocalPlayer.Character["GMan5Attacks"].Escape.Value = true
game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value = true
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "astro_teleport"})
FlySpeed = 500
local aura = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game:GetService("ReplicatedStorage").Assets.Specialized.GMAN.Aura.Impact2, game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
local bodyvel = Instance.new("BodyVelocity", aura)
bodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
bodyvel.Velocity = Vector3.new(0, 0, 0)
task.spawn(function()
while game.Players.LocalPlayer.Character["GMan5Attacks"].Escape.Value == true and FlySpeed == 500 and GMan5Alive == true do
if aura then
aura.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
end
game:GetService("RunService").Heartbeat:Wait()
end
if aura then
game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(aura, false)
end
end)
task.wait(1)
if GMan5Alive == false then return end
game.Players.LocalPlayer.Character["GMan5Attacks"].DoingAtk.Value = false
task.wait(5)
if GMan5Alive == false then return end
FlySpeed = 150
game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {game.Players.LocalPlayer.Character.HumanoidRootPart, "astro_cam:di"})
task.wait(8)
if GMan5Alive == false then return end
game.Players.LocalPlayer.Character["GMan5Attacks"].Escape.Value = false
end
end)

_G.CalcFly = game:GetService("RunService").RenderStepped:Connect(function()
    if game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("FlyingScript") then
		if canFly.Value == true then
        	game.Players.LocalPlayer.Character.Humanoid:ChangeState(6)
        	game.Players.LocalPlayer.Character.HumanoidRootPart.BodyGyro.CFrame = game.workspace.Camera.CFrame
        	game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity, TweenInfo.new(0.3), {
            	Velocity = FlyMove() * FlySpeed;
        	}):Play()
		else
			game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity.Velocity = Vector3.new(0, 0, 0)
		end
	else
		CalcFly:Disconnect()
    end
end)
_G.GMan5Died = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
GMan5Alive = false
if _G.CalcFly then _G.CalcFly:Disconnect() end
for _, v in ipairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
	if v.Name == "IdleFlying" then
		v:Stop()
		v:Destroy()
	end
end
_G.GMan5ControlButtons:Disconnect()
G2L["1"]:Destroy()
game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("FlyingScript"):Destroy()
game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity:Destroy()
game.Players.LocalPlayer.Character.HumanoidRootPart.BodyGyro:Destroy()
end)
_G.GMan5Spawned:Disconnect()
end
end)
game:GetService("ReplicatedStorage").UIRemotes.Morph:InvokeServer(game:GetService("ReplicatedStorage").Modules.Util["Math.c2"].Storage.GManBathroom5, false, false)
end)

