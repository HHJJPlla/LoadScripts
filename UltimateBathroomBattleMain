game.Players.LocalPlayer["LASER:VARS"].fuel.Value = MaxFuel
end)
checklfuelalive = game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
inflfuelcheck:Disconnect()
end)
end
end)
end
