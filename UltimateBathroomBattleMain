local G2L = {};
local boss
G2L["1"] = Instance.new("ScreenGui", game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"));
G2L["1"]["Name"] = [[BossEditGUI]];
G2L["1"]["ZIndexBehavior"] = Enum.ZIndexBehavior.Sibling;
G2L["1"]["ResetOnSpawn"] = false;
G2L["1"]["Enabled"] = false;

G2L["2"] = Instance.new("Frame", G2L["1"]);
G2L["2"]["BorderSizePixel"] = 0;
G2L["2"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["2"]["Size"] = UDim2.new(0, 200, 0, 50);
G2L["2"]["Position"] = UDim2.new(0.445, 0, 0.9, 0);
G2L["2"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["2"]["BackgroundTransparency"] = 0.9;
G2L["3"] = Instance.new("UICorner", G2L["2"]);

G2L["4"] = Instance.new("TextLabel", G2L["2"]);
G2L["4"]["BorderSizePixel"] = 0;
G2L["4"]["TextSize"] = 12;
G2L["4"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["4"]["FontFace"] = Font.new([[rbxasset://fonts/families/Arial.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["4"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["4"]["BackgroundTransparency"] = 0.8;
G2L["4"]["Size"] = UDim2.new(0, 175, 0, 25);
G2L["4"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["4"]["Text"] = [[Nothing here. Press Search]];
G2L["4"]["Name"] = [[nameboss]];
G2L["5"] = Instance.new("UICorner", G2L["4"]);

G2L["6"] = Instance.new("ImageButton", G2L["2"]);
G2L["6"]["BorderSizePixel"] = 0;
G2L["6"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["6"]["Image"] = [[http://www.roblox.com/asset/?id=118685771787843]];
G2L["6"]["Size"] = UDim2.new(0, 25, 0, 25);
G2L["6"]["BackgroundTransparency"] = 0.8;
G2L["6"]["Name"] = [[searchboss]];
G2L["6"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["6"]["Position"] = UDim2.new(0.875, 0, 0, 0);
G2L["6"].MouseButton1Click:Connect(function()
	for i, v in pairs(workspace:GetChildren()) do
		if v.Name == "GManBathroom5" then
			G2L["4"].Text = v.Name
			boss = v.Name
			if not v:FindFirstChild("GivenGod") then
				G2L["e"]["ImageColor3"] = Color3.fromRGB(0, 180, 0)
			else
				G2L["e"]["ImageColor3"] = Color3.fromRGB(255, 0, 0)
			end
			if v:FindFirstChild("andIlookforya") then
				G2L["10"]["ImageColor3"] = Color3.fromRGB(200, 200, 200)
				v.LOOKBOSS:Destroy()
				v.BOSSLABEL:Destroy()
				v.andIlookforya:Destroy()
				if CalcBossHP then CalcBossHP:Disconnect() end
			end
		end
	end
end)
G2L["7"] = Instance.new("UICorner", G2L["6"]);

G2L["8"] = Instance.new("ImageButton", G2L["2"]);
G2L["8"]["BorderSizePixel"] = 0;
G2L["8"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["8"]["Image"] = [[http://www.roblox.com/asset/?id=6723742952]];
G2L["8"]["Size"] = UDim2.new(0, 25, 0, 25);
G2L["8"]["BackgroundTransparency"] = 0.8;
G2L["8"]["Name"] = [[tpbosstome]];
G2L["8"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["8"]["Position"] = UDim2.new(0, 0, 0.5, 0);
G2L["8"].MouseButton1Click:Connect(function()
	if boss ~= nil and workspace:FindFirstChild(boss) and workspace[boss].PrimaryPart and game.Players.LocalPlayer.Character ~= nil and game.Players.LocalPlayer.Character.PrimaryPart then
		game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", workspace[boss].PrimaryPart, game.Players.LocalPlayer.Character.PrimaryPart.CFrame)
	end
end)
G2L["9"] = Instance.new("UICorner", G2L["8"]);

G2L["a"] = Instance.new("ImageButton", G2L["2"]);
G2L["a"]["BorderSizePixel"] = 0;
G2L["a"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["a"]["Image"] = [[http://www.roblox.com/asset/?id=13288270182]];
G2L["a"]["Size"] = UDim2.new(0, 25, 0, 25);
G2L["a"]["BackgroundTransparency"] = 0.8;
G2L["a"]["Name"] = [[sethpboss]];
G2L["a"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["a"]["Position"] = UDim2.new(0.75, 0, 0.5, 0);
G2L["a"].MouseButton1Click:Connect(function()
	if boss ~= nil and workspace:FindFirstChild(boss) and workspace[boss]:FindFirstChildOfClass("Humanoid") then
		if tonumber(G2L["c"].Text) == nil then
			G2L["c"].Text = "20000"
		end
		if tonumber(G2L["c"].Text) > 20000 or tonumber(G2L["c"].Text) < 0 then
			G2L["c"].Text = "20000"
		end
		game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SET:ZE:OLD:HP", workspace[boss].Humanoid, G2L["c"].Text)
	end
end)
G2L["b"] = Instance.new("UICorner", G2L["a"]);

G2L["c"] = Instance.new("TextBox", G2L["a"]);
G2L["c"]["CursorPosition"] = -1;
G2L["c"]["BorderSizePixel"] = 0;
G2L["c"]["TextSize"] = 18;
G2L["c"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["c"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["c"]["FontFace"] = Font.new([[rbxasset://fonts/families/SourceSansPro.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["c"]["Size"] = UDim2.new(0, 50, 0, 25);
G2L["c"]["Position"] = UDim2.new(-2, 0, 0, 0);
G2L["c"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["c"]["Text"] = [[20000]];
G2L["c"]["BackgroundTransparency"] = 0.8;
G2L["d"] = Instance.new("UICorner", G2L["c"]);

G2L["e"] = Instance.new("ImageButton", G2L["2"]);
G2L["e"]["BorderSizePixel"] = 0;
G2L["e"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["e"]["ImageColor3"] = Color3.fromRGB(255, 0, 0);
G2L["e"]["Image"] = [[http://www.roblox.com/asset/?id=12988744896]];
G2L["e"]["Size"] = UDim2.new(0, 25, 0, 25);
G2L["e"]["BackgroundTransparency"] = 0.8;
G2L["e"]["Name"] = [[godboss]];
G2L["e"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["e"]["Position"] = UDim2.new(0.875, 0, 0.5, 0);
G2L["e"].MouseButton1Click:Connect(function()
	if boss ~= nil and workspace:FindFirstChild(boss) then
		local bosspa = workspace[boss]
		if not bosspa:FindFirstChild("GivenGod") then
			G2L["e"]["ImageColor3"] = Color3.fromRGB(0, 180, 0)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, bosspa)
			Instance.new("BoolValue", bosspa).Name = "GivenGod"
		else
			G2L["e"]["ImageColor3"] = Color3.fromRGB(255, 0, 0)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, bosspa)
			bosspa.GivenGod:Destroy()
		end
	end
end)
G2L["f"] = Instance.new("UICorner", G2L["e"]);

G2L["10"] = Instance.new("ImageButton", G2L["2"]);
G2L["10"]["BorderSizePixel"] = 0;
G2L["10"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L["10"]["ImageColor3"] = Color3.fromRGB(200, 200, 200)
G2L["10"]["Image"] = [[http://www.roblox.com/asset/?id=13321848320]];
G2L["10"]["Size"] = UDim2.new(0, 25, 0, 25);
G2L["10"]["BackgroundTransparency"] = 0.8;
G2L["10"]["Name"] = [[checkboss]];
G2L["10"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["10"]["Position"] = UDim2.new(0.125, 0, 0.5, 0);
G2L["e"].MouseButton1Click:Connect(function()
	if boss ~= nil and workspace:FindFirstChild(boss) and workspace[boss].PrimaryPart and workspace[boss]:FindFirstChildOfClass("Humanoid") then
		local bosspa = workspace[boss]
		if not bosspa:FindFirstChild("andIlookforya") then
			G2L["10"]["ImageColor3"] = Color3.fromRGB(255, 255, 255)
			local seeya = Instance.new("Highlight", bosspa)
			seeya.Name = "LOOKBOSS"
			local billb = Instance.new("BillboardGui", bosspa.PrimaryPart)
			billb.AlwaysOnTop = true
			billb.Size = UDim2.new(0, 50, 0, 50)
			billb.StudsOffset = Vector3.new(0, 15, 0)
			billb.Name = "BOSSLABEL"
			local icon = Instance.new("ImageLabel", billb)
			icon.BackgroundTransparency = 1
			icon.Position = UDim2.new(0.2, 0, 0, 0)
			icon.Size = UDim2.new(0, 30, 0, 30)
			icon.Image = "http://www.roblox.com/asset/?id=16682879119"
			icon.Name = "iconBoss"
			local hptext = Instance.new("TextLabel", billb)
			hptext.BackgroundTransparency = 1
			hptext.Position = UDim2.new(0, 0, 0.7, 0)
			hptext.Size = UDim2.new(0, 50, 0, 10)
			hptext.Text = tostring(bosspa.Humanoid.Health)
			hptext.Name = "hpBoss"
			local healthpast = false
			if bosspa.Humanoid.Health < 1000 then
				healthpast = true
				hptext.TextColor3 = Color3.fromRGB(255, 0, 0)
			else
				hptext.TextColor3 = Color3.fromRGB(255, 255, 255)
			end
			hptext.TextSize = 15
			CalcBossHP = bosspa.Humanoid.HealthChanged:Connect(function(health)
				if hptext ~= nil then
					hptext.Text = tostring(health)
					if healthpast == false and health < 1000 then
						healthpast = true
						hptext.TextColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
			end)
			Instance.new("BoolValue", bosspa).Name = "andIlookforya"
		else
			G2L["10"]["ImageColor3"] = Color3.fromRGB(200, 200, 200)
			bosspa.LOOKBOSS:Destroy()
			bosspa.BOSSLABEL:Destroy()
			bosspa.andIlookforya:Destroy()
			if CalcBossHP then CalcBossHP:Disconnect() end
		end
	end
end)
G2L["11"] = Instance.new("UICorner", G2L["10"]);

ai:Toggle("Boss Editing", false, function(bool)
G2L["1"].Enabled = bool
end)

local G2L = {};
G2L["1"] = Instance.new("ScreenGui", game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"));
G2L["1"]["Name"] = [[SCRIPT_GCloneRemote]];
G2L["1"]["ZIndexBehavior"] = Enum.ZIndexBehavior.Sibling;
G2L["1"].ResetOnSpawn = false
G2L["1"].Enabled = false

G2L["2"] = Instance.new("Frame", G2L["1"]);
G2L["2"]["BorderSizePixel"] = 0;
G2L["2"]["BackgroundColor3"] = Color3.fromRGB(81, 81, 81);
G2L["2"]["Size"] = UDim2.new(0, 150, 0, 250);
G2L["2"]["Position"] = UDim2.new(0.48295, 242, 0.66201, 33);
G2L["2"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["3"] = Instance.new("UICorner", G2L["2"]);
G2L["4"] = Instance.new("UIGradient", G2L["2"]);
G2L["4"]["Rotation"] = 90;
G2L["4"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(81, 81, 81))};

G2L["5"] = Instance.new("TextLabel", G2L["2"]);
G2L["5"]["BorderSizePixel"] = 0;
G2L["5"]["TextSize"] = 16;
G2L["5"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
G2L["5"]["FontFace"] = Font.new([[rbxasset://fonts/families/DenkOne.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["5"]["TextColor3"] = Color3.fromRGB(211, 211, 211);
G2L["5"]["BackgroundTransparency"] = 1;
G2L["5"]["Size"] = UDim2.new(0, 150, 0, 25);
G2L["5"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["5"]["Text"] = [[GMan Clone Remote]];
G2L["5"]["Name"] = [[named]];
G2L["5"]["Position"] = UDim2.new(0, 0, -0.004, 0);

G2L["6"] = Instance.new("TextButton", G2L["2"]);
G2L["6"]["BorderSizePixel"] = 0;
G2L["6"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["6"]["TextSize"] = 14;
G2L["6"]["BackgroundColor3"] = Color3.fromRGB(51, 51, 51);
G2L["6"]["FontFace"] = Font.new([[rbxasset://fonts/families/SourceSansPro.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["6"]["Size"] = UDim2.new(0, 130, 0, 25);
G2L["6"]["Name"] = [[releaselaser]];
G2L["6"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["6"]["Text"] = [[Release Laser G]];
G2L["6"]["Position"] = UDim2.new(0.06667, 0, 0.124, 0);

lastHpLaserClone = 3000
local releasedLaser = Instance.new("BoolValue", G2L["6"])
releasedLaser.Name = "releasedLaser"
releasedLaser.Value = false
G2L["6"].MouseButton1Click:Connect(function()
	releasedLaser.Value = not releasedLaser.Value
	if releasedLaser.Value == true then
		game:GetService("ReplicatedStorage").SFX.OperationSelect:Play()
		G2L["6"]["Text"] = [[Stop Laser G]]
		G2L["6"].Interactable = false
		local AI_Speed = -5
		local AI_Target = nil
		local AI_Does = false
		pcall(function()
			game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game.ReplicatedStorage.MorphShow.LaserRingGClone, Vector3.new(0, 0, 0))
		end)
		if workspace.Debris:WaitForChild("LaserRingGClone", 5) then
			local LaserGHelper = workspace.Debris:WaitForChild("LaserRingGClone", 5)
			if not workspace:FindFirstChild("CustomAIs") then
				local customAIsFolder = Instance.new("Folder", workspace)
				customAIsFolder.Name = "CustomAIs"
			end
			LaserGHelper.Parent = workspace.CustomAIs
			local AIFolder = Instance.new("Folder", LaserGHelper)
			AIFolder.Name = "AI"
			local AIActive = Instance.new("BoolValue", AIFolder)
			AIActive.Name = "AI:Enabled"
			AIActive.Value = true
			local AICombat = Instance.new("StringValue", AIFolder)
			AICombat.Name = "AI:Combat"
			AICombat.Value = "Searching"
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, LaserGHelper)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM:ASTRO", LaserGHelper)
			task.spawn(function()
				if LaserGHelper:WaitForChild("BathroomTeam", 5) and LaserGHelper ~= nil then
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", LaserGHelper:FindFirstChild("AstroTeam"), Vector3.new(0,-600,0))
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", LaserGHelper:FindFirstChild("LensmenTeam"), Vector3.new(0,-600,0))
					task.wait(0.2)
					if LaserGHelper ~= nil then
						game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, LaserGHelper)
					end
				end
			end)
			game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(LaserGHelper.PrimaryPart, true)
			game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", LaserGHelper.PrimaryPart, CFrame.new(game.Players.LocalPlayer.Character.PrimaryPart.Position.X, 230, game.Players.LocalPlayer.Character.PrimaryPart.Position.Z))
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {LaserGHelper.PrimaryPart, "Starrttt"})
			if LaserGHelper.PrimaryPart:WaitForChild("Starrttt", 5) then
				G2L["6"].Interactable = true
				task.spawn(function()
					while LaserGHelper and LaserGHelper.PrimaryPart and AIActive.Value == true do
						local getEnemy = GetNearestPlayer(900, LaserGHelper.PrimaryPart, "LensmenTeam", false)
						if getEnemy ~= nil then
							if (LaserGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude <= 500 and (LaserGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude >= 150 then
								AICombat.Value = "Attack"
								AI_Target = getEnemy
							elseif (LaserGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude > 500 then
								AICombat.Value = "Chase"
								AI_Target = getEnemy
							elseif (LaserGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude < 150 then
								AICombat.Value = "Flee"
								AI_Target = getEnemy
							end
						else
							AI_Target = nil
							AICombat.Value = "Searching"
						end
						game:GetService("RunService").Heartbeat:Wait()
					end
				end)
				task.spawn(function()
					DoAction = AICombat:GetPropertyChangedSignal("Value"):Connect(function()
						if AI_Does == true then return end
						if AICombat.Value == "Attack" and LaserGHelper:FindFirstChild("Head") and AI_Target ~= nil and not LaserGHelper.Head:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(LaserGHelper.Head, true)
							game.ReplicatedStorage.CombatRemotes.LaserRework:FireServer("LaserSound", LaserGHelper.PrimaryPart, true)
							for i, v in pairs(LaserGHelper:GetChildren()) do
								if v.Name:match("Thing") then
									game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(v, true)
								end
							end
							if LaserGHelper.Head:WaitForChild("STARMANBUFF", 5) then
								repeat
									if LaserGHelper ~= nil and LaserGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart and not AI_Target:FindFirstChild("DeadS") then
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", LaserGHelper.PrimaryPart, CFrame.lookAt(LaserGHelper.PrimaryPart.Position, AI_Target.PrimaryPart.Position))
										if not AI_Target:FindFirstChild("LaserGHelper_hitcd") then
											game.ReplicatedStorage.CombatRemotes.AIFramework:FireServer("DAMAGE", LaserGHelper, AI_Target.Humanoid, 75, AI_Target)
											local LaserCD = Instance.new("BoolValue", AI_Target)
											LaserCD.Name = "LaserGHelper_hitcd"
											game:GetService("Debris"):AddItem(LaserCD, 0.6)
										end
										for i, v in pairs(LaserGHelper:GetChildren()) do
											if v.Name:match("Thing") then
												game.ReplicatedStorage.CombatRemotes.LaserRework:FireServer("LaserSnapToPos", v, AI_Target.PrimaryPart.Position)
											end
										end
									end
									task.wait(0.1)
								until AICombat.Value ~= "Attack" or LaserGHelper == nil
								if LaserGHelper ~= nil then
									for i, v in pairs(LaserGHelper:GetChildren()) do
										if v.Name:match("Thing") then
											game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(v, false)
										end
									end
									game.ReplicatedStorage.CombatRemotes.LaserRework:FireServer("LaserSound", LaserGHelper.PrimaryPart, false)
									game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {LaserGHelper.PrimaryPart, "laserend"})
									game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(LaserGHelper.Head, false)
								end
							end
						elseif AICombat.Value == "Chase" and LaserGHelper:FindFirstChild("HumanoidRootPart") and AI_Target ~= nil and not LaserGHelper.HumanoidRootPart:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							AI_Speed = -5
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(LaserGHelper.HumanoidRootPart, true)
							if LaserGHelper.HumanoidRootPart:WaitForChild("STARMANBUFF", 5) then
								if LaserGHelper and LaserGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
									game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", LaserGHelper.PrimaryPart, CFrame.lookAt(LaserGHelper.PrimaryPart.Position, Vector3.new(AI_Target.PrimaryPart.Position.X, LaserGHelper.PrimaryPart.Position.Y, AI_Target.PrimaryPart.Position.Z)) * CFrame.new(0, 0, AI_Speed))
								end
								moveChase = LaserGHelper.PrimaryPart:GetPropertyChangedSignal("Position"):Connect(function()
									if LaserGHelper and LaserGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
										if AI_Speed >= -45 then
											AI_Speed -= 2.5
										end
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", LaserGHelper.PrimaryPart, CFrame.lookAt(LaserGHelper.PrimaryPart.Position, Vector3.new(AI_Target.PrimaryPart.Position.X, LaserGHelper.PrimaryPart.Position.Y, AI_Target.PrimaryPart.Position.Z)) * CFrame.new(0, 0, AI_Speed))
									end	
								end)
								repeat game:GetService("RunService").Heartbeat:Wait() until AICombat.Value ~= "Chase" or LaserGHelper == nil
								if moveChase then moveChase:Disconnect() end
								if LaserGHelper ~= nil then
									game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(LaserGHelper.HumanoidRootPart, false)
								end
							end
						elseif AICombat.Value == "Flee" and LaserGHelper:FindFirstChild("boilet99") and AI_Target ~= nil and not LaserGHelper.boilet99:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							AI_Speed = -5
							local calcBackUp = 60
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(LaserGHelper.boilet99, true)
							local escapeFrom = AI_Target.PrimaryPart.Position
							if LaserGHelper.boilet99:WaitForChild("STARMANBUFF", 5) then
								local FleeStad = 5
								if LaserGHelper and LaserGHelper.PrimaryPart then
									game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", LaserGHelper.HumanoidRootPart, CFrame.lookAt(LaserGHelper.PrimaryPart.Position, Vector3.new(escapeFrom.X, LaserGHelper.PrimaryPart.Position.Y, escapeFrom.Z)) * CFrame.new(0, 0, calcBackUp))
								end
								moveChase = LaserGHelper.PrimaryPart:GetPropertyChangedSignal("Position"):Connect(function()
									if FleeStad ~= 0 and LaserGHelper and LaserGHelper.PrimaryPart then
										calcBackUp = calcBackUp - 12.5
										FleeStad -= 1
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", LaserGHelper.HumanoidRootPart, CFrame.lookAt(LaserGHelper.PrimaryPart.Position, Vector3.new(escapeFrom.X, LaserGHelper.PrimaryPart.Position.Y, escapeFrom.Z)) * CFrame.new(0, 0, calcBackUp))
									else
										moveChase:Disconnect()
										if LaserGHelper ~= nil then
											game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(LaserGHelper.boilet99, false)
										end
									end
								end)
							end
						end
					end)
					removedAction = LaserGHelper.DescendantRemoving:Connect(function(descendant)
						if descendant.Name == "STARMANBUFF" then
							if AICombat then
								AICombat.Value = "None"
							end
							AI_Does = false
						end
					end)
				end)
				task.spawn(function()
					if LaserGHelper:WaitForChild("Humanoid", 5) then
						if lastHpLaserClone ~= 3000 then
							game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SET:ZE:OLD:HP", LaserGHelper.Humanoid, lastHpLaserClone)
						end
						lasDied = LaserGHelper.Humanoid.HealthChanged:Connect(function(health)
							G2L["9"]["Text"] = "Health: "..math.floor(health).."/3000"
							if health == 0 then
								G2L["6"].Text = "Signal Lost..." G2L["6"].Interactable = false G2L["8"]:Destroy()
								G2L["a"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(240, 0, 0))}
								game:GetService("ReplicatedStorage").SFX.ddd:Play()
								repeat task.wait() until AI_Does == false
								AIActive.Value = false
								removedAction:Disconnect()
								DoAction:Disconnect()
								if moveChase then moveChase:Disconnect() end
								AICombat.Value = "Dead"
								G2L["9"].Text = "Dead"
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", LaserGHelper.skinnedmeshhead, 1)
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SHOULDER:VISIBILITY", LaserGHelper.skinnedmeshhead, 1)
								lasDied:Disconnect()
								task.wait(5)
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", LaserGHelper, Vector3.new(0,-600,0))
							end
						end)
					end
				end)
			end
		end
	else
		G2L["6"]["Text"] = [[Release Laser G]]
		game:GetService("ReplicatedStorage").SFX.OperationSelect:Play()
		if workspace.CustomAIs:FindFirstChild("LaserRingGClone") then
			workspace.CustomAIs.LaserRingGClone.AI["AI:Enabled"].Value = false
			removedAction:Disconnect()
			DoAction:Disconnect()
			if moveChase then moveChase:Disconnect() end
			lasDied:Disconnect()
			workspace.CustomAIs.LaserRingGClone.AI["AI:Combat"].Value = "CallBack"
			lastHpLaserClone = math.floor(workspace.CustomAIs.LaserRingGClone.Humanoid.Health)
			G2L["9"]["Text"] = "Health: "..math.floor(workspace.CustomAIs.LaserRingGClone.Humanoid.Health).."/3000"
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", workspace.CustomAIs.LaserRingGClone, Vector3.new(0,-600,0))
		end
	end
end)
G2L["7"] = Instance.new("UICorner", G2L["6"]);
G2L["8"] = Instance.new("UIGradient", G2L["6"]);
G2L["8"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.690, Color3.fromRGB(255, 255, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 255, 0))};

G2L["9"] = Instance.new("TextLabel", G2L["6"]);
G2L["9"]["BorderSizePixel"] = 0;
G2L["9"]["TextSize"] = 14;
G2L["9"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
G2L["9"]["FontFace"] = Font.new([[rbxasset://fonts/families/Ubuntu.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["9"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["9"]["BackgroundTransparency"] = 1;
G2L["9"]["Size"] = UDim2.new(0, 150, 0, 15);
G2L["9"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["9"]["Text"] = [[Health: 3000/3000]];
G2L["9"]["Name"] = [[health]];
G2L["9"]["Position"] = UDim2.new(-0.07692, 0, 0.982, 0);
G2L["a"] = Instance.new("UIGradient", G2L["9"]);
G2L["a"]["Rotation"] = 90;
G2L["a"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(0, 161, 0))};

G2L["b"] = Instance.new("TextButton", G2L["2"]);
G2L["b"]["BorderSizePixel"] = 0;
G2L["b"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["b"]["TextSize"] = 14;
G2L["b"]["BackgroundColor3"] = Color3.fromRGB(51, 51, 51);
G2L["b"]["FontFace"] = Font.new([[rbxasset://fonts/families/SourceSansPro.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["b"]["Size"] = UDim2.new(0, 130, 0, 25);
G2L["b"]["Name"] = [[releaserocket]];
G2L["b"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["b"]["Text"] = [[Release Rocketeer G]];
G2L["b"]["Position"] = UDim2.new(0.067, 0, 0.294, 0);

lastHpRocketClone = 3000
local releasedRocket = Instance.new("BoolValue", G2L["b"])
releasedRocket.Name = "releasedRocket"
releasedRocket.Value = false
G2L["b"].MouseButton1Click:Connect(function()
	releasedRocket.Value = not releasedRocket.Value
	if releasedRocket.Value == true then
		game:GetService("ReplicatedStorage").SFX.OperationSelect:Play()
		G2L["b"]["Text"] = [[Stop Rocketeer G]]
		G2L["b"].Interactable = false
		local AI_Speed = -5
		local AI_Target = nil
		local AI_Does = false
		pcall(function()
			game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game.ReplicatedStorage.MorphShow.QuadRocketeerGClone, Vector3.new(0, 0, 0))
		end)
		if workspace.Debris:WaitForChild("QuadRocketeerGClone", 5) then
			local RockGHelper = workspace.Debris:WaitForChild("QuadRocketeerGClone", 5)
			if not workspace:FindFirstChild("CustomAIs") then
				local customAIsFolder = Instance.new("Folder", workspace)
				customAIsFolder.Name = "CustomAIs"
			end
			RockGHelper.Parent = workspace.CustomAIs
			local AIFolder = Instance.new("Folder", RockGHelper)
			AIFolder.Name = "AI"
			local AIActive = Instance.new("BoolValue", AIFolder)
			AIActive.Name = "AI:Enabled"
			AIActive.Value = true
			local AICombat = Instance.new("StringValue", AIFolder)
			AICombat.Name = "AI:Combat"
			AICombat.Value = "Searching"
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, RockGHelper)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM:ASTRO", RockGHelper)
			task.spawn(function()
				if RockGHelper:WaitForChild("BathroomTeam", 5) and RockGHelper ~= nil then
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", RockGHelper:FindFirstChild("AstroTeam"), Vector3.new(0,-600,0))
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", RockGHelper:FindFirstChild("LensmenTeam"), Vector3.new(0,-600,0))
					task.wait(0.2)
					if RockGHelper ~= nil then
						game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, RockGHelper)
					end
				end
			end)
			game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(RockGHelper.PrimaryPart, true)
			game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", RockGHelper.PrimaryPart, CFrame.new(game.Players.LocalPlayer.Character.PrimaryPart.Position.X, 230, game.Players.LocalPlayer.Character.PrimaryPart.Position.Z))
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {RockGHelper.PrimaryPart, "Starrttt"})
			if RockGHelper.PrimaryPart:WaitForChild("Starrttt", 5) then
				G2L["b"].Interactable = true
				task.spawn(function()
					while RockGHelper and RockGHelper.PrimaryPart and AIActive.Value == true do
						local getEnemy = GetNearestPlayer(750, RockGHelper.PrimaryPart, "LensmenTeam", false)
						if getEnemy ~= nil then
							if (RockGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude <= 375 and (RockGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude >= 150 then
								AICombat.Value = "Attack"
								AI_Target = getEnemy
							elseif (RockGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude > 375 then
								AICombat.Value = "Chase"
								AI_Target = getEnemy
							elseif (RockGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude < 150 then
								AICombat.Value = "Flee"
								AI_Target = getEnemy
							end
						else
							AI_Target = nil
							AICombat.Value = "Searching"
						end
						game:GetService("RunService").Heartbeat:Wait()
					end
				end)
				task.spawn(function()
					DoAction = AICombat:GetPropertyChangedSignal("Value"):Connect(function()
						if AI_Does == true then return end
						if AICombat.Value == "Attack" and RockGHelper:FindFirstChild("Head") and AI_Target ~= nil and not RockGHelper.Head:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RockGHelper.Head, true)
							if RockGHelper.Head:WaitForChild("STARMANBUFF", 5) then
								repeat
									if not AICombat:FindFirstChild("Fire") then
										local Firing = Instance.new("BoolValue", AICombat)
										Firing.Name = "Fire"
										game:GetService("Debris"):AddItem(Firing, 0.5)
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", RockGHelper.PrimaryPart, CFrame.lookAt(RockGHelper.PrimaryPart.Position, AI_Target.PrimaryPart.Position))
										task.spawn(function()
											local RocketToLaunch = nil
											for i, v in pairs(RockGHelper:GetChildren()) do
												if v.Name:match("RocketPos") then
													if v.Transparency == 0 then
														local RocketInvisible = v
														RocketToLaunch = v
														RocketInvisible.Transparency = 1
														task.spawn(function()
															task.wait(10)
															RocketInvisible.Transparency = 0
														end)
														break
													end
												end
											end
											if RocketToLaunch ~= nil and RockGHelper ~= nil and RockGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
												local GRocket = game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(RocketToLaunch, RocketToLaunch.Position)
												GRocket.CanCollide = false
												game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {RockGHelper.PrimaryPart, "rocket_ll_shoot"})
												game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("INIT:FLY:EFFECT", GRocket)
												local rbodyvel = Instance.new("BodyVelocity", GRocket)
												rbodyvel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
												if AI_Target ~= nil and AI_Target.PrimaryPart then
													GRocket.CFrame = CFrame.lookAt(GRocket.Position, AI_Target.PrimaryPart.Position)
												else
													GRocket.CFrame = RocketToLaunch.CFrame
												end
												rbodyvel.Velocity = GRocket.CFrame.LookVector * 600
												local debouncer = false
												local RocketHit = GRocket.Touched:Connect(function(part)
													if debouncer == false and not part:IsDescendantOf(RockGHelper) then
														debouncer = true
														local realExplosionPos = GRocket.CFrame
														game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(GRocket, true)
														if GRocket:WaitForChild("STARMANBUFF", 5) then
															for i, v in pairs(workspace:GetChildren()) do
																if v:IsA("Model") and v.PrimaryPart and v:FindFirstChild("LensmenTeam") and v:FindFirstChildOfClass("Humanoid") and (GRocket.Position-v.PrimaryPart.Position).Magnitude <= 95 then
																	game.ReplicatedStorage.CombatRemotes.AIFramework:FireServer("DAMAGE", RockGHelper, v.Humanoid, 300, v)
																end
															end
														end
														game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.WARHEAD.boom, realExplosionPos, "pumpkin_explode"..math.random(1,3))
														game.ReplicatedStorage.CombatRemotes["_s_WJSQJDWJQDUJQWJU"]:FireServer(GRocket)
													end
												end)
											end
										end)
									end
									game:GetService("RunService").Heartbeat:Wait()
								until AICombat.Value ~= "Attack" or RockGHelper == nil
								if RockGHelper ~= nil then
									game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RockGHelper.Head, false)
								end
							end
						elseif AICombat.Value == "Chase" and RockGHelper:FindFirstChild("HumanoidRootPart") and AI_Target ~= nil and not RockGHelper.HumanoidRootPart:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							AI_Speed = -5
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RockGHelper.HumanoidRootPart, true)
							if RockGHelper.HumanoidRootPart:WaitForChild("STARMANBUFF", 5) then
								if RockGHelper and RockGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
									game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", RockGHelper.PrimaryPart, CFrame.lookAt(RockGHelper.PrimaryPart.Position, Vector3.new(AI_Target.PrimaryPart.Position.X, RockGHelper.PrimaryPart.Position.Y, AI_Target.PrimaryPart.Position.Z)) * CFrame.new(0, 0, AI_Speed))
								end
								moveChase = RockGHelper.PrimaryPart:GetPropertyChangedSignal("Position"):Connect(function()
									if RockGHelper and RockGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
										if AI_Speed >= -45 then
											AI_Speed -= 2.5
										end
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", RockGHelper.PrimaryPart, CFrame.lookAt(RockGHelper.PrimaryPart.Position, Vector3.new(AI_Target.PrimaryPart.Position.X, RockGHelper.PrimaryPart.Position.Y, AI_Target.PrimaryPart.Position.Z)) * CFrame.new(0, 0, AI_Speed))
									end	
								end)
								repeat game:GetService("RunService").Heartbeat:Wait() until AICombat.Value ~= "Chase" or RockGHelper == nil
								if moveChase then moveChase:Disconnect() end
								if RockGHelper ~= nil then
									game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RockGHelper.HumanoidRootPart, false)
								end
							end
						elseif AICombat.Value == "Flee" and RockGHelper:FindFirstChild("boilet99") and AI_Target ~= nil and not RockGHelper.boilet99:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							AI_Speed = -5
							local calcBackUp = 60
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RockGHelper.boilet99, true)
							local escapeFrom = AI_Target.PrimaryPart.Position
							if RockGHelper.boilet99:WaitForChild("STARMANBUFF", 5) then
								local FleeStad = 5
								if RockGHelper and RockGHelper.PrimaryPart then
									game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", RockGHelper.HumanoidRootPart, CFrame.lookAt(RockGHelper.PrimaryPart.Position, Vector3.new(escapeFrom.X, RockGHelper.PrimaryPart.Position.Y, escapeFrom.Z)) * CFrame.new(0, 0, calcBackUp))
								end
								moveChase = RockGHelper.PrimaryPart:GetPropertyChangedSignal("Position"):Connect(function()
									if FleeStad ~= 0 and RockGHelper and RockGHelper.PrimaryPart then
										calcBackUp = calcBackUp - 12.5
										FleeStad -= 1
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", RockGHelper.HumanoidRootPart, CFrame.lookAt(RockGHelper.PrimaryPart.Position, Vector3.new(escapeFrom.X, RockGHelper.PrimaryPart.Position.Y, escapeFrom.Z)) * CFrame.new(0, 0, calcBackUp))
									else
										moveChase:Disconnect()
										if RockGHelper ~= nil then
											game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(RockGHelper.boilet99, false)
										end
									end
								end)
							end
						end
					end)
				end)
				removedAction = RockGHelper.DescendantRemoving:Connect(function(descendant)
					if descendant.Name == "STARMANBUFF" then
						if AICombat then
							AICombat.Value = "None"
						end
						AI_Does = false
					end
				end)
				task.spawn(function()
					if RockGHelper:WaitForChild("Humanoid", 5) then
						if lastHpRocketClone ~= 3000 then
							game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SET:ZE:OLD:HP", RockGHelper.Humanoid, lastHpRocketClone)
						end
						rockDied = RockGHelper.Humanoid.HealthChanged:Connect(function(health)
							G2L["e"]["Text"] = "Health: "..math.floor(health).."/3000"
							if health == 0 then
								G2L["b"].Text = "Signal Lost..." G2L["b"].Interactable = false G2L["d"]:Destroy()
								G2L["f"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(240, 0, 0))}
								game:GetService("ReplicatedStorage").SFX.ddd:Play()
								repeat task.wait() until AI_Does == false
								AIActive.Value = false
								removedAction:Disconnect()
								DoAction:Disconnect()
								if moveChase then moveChase:Disconnect() end
								AICombat.Value = "Dead"
								G2L["e"].Text = "Dead"
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", RockGHelper.skinnedmeshhead, 1)
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SHOULDER:VISIBILITY", RockGHelper.skinnedmeshhead, 1)
								rockDied:Disconnect()
								task.wait(5)
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", RockGHelper, Vector3.new(0,-600,0))
							end
						end)
					end
				end)
			end
		end
	else
		G2L["b"]["Text"] = [[Release Rocketeer G]]
		game:GetService("ReplicatedStorage").SFX.OperationSelect:Play()
		if workspace.CustomAIs:FindFirstChild("QuadRocketeerGClone") then
			workspace.CustomAIs.QuadRocketeerGClone.AI["AI:Enabled"].Value = false
			removedAction:Disconnect()
			DoAction:Disconnect()
			if moveChase then moveChase:Disconnect() end
			rockDied:Disconnect()
			workspace.CustomAIs.QuadRocketeerGClone.AI["AI:Combat"].Value = "CallBack"
			lastHpRocketClone = math.floor(workspace.CustomAIs.QuadRocketeerGClone.Humanoid.Health)
			G2L["e"]["Text"] = "Health: "..math.floor(workspace.CustomAIs.QuadRocketeerGClone.Humanoid.Health).."/3000"
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", workspace.CustomAIs.QuadRocketeerGClone, Vector3.new(0,-600,0))
		end
	end
end)
G2L["c"] = Instance.new("UICorner", G2L["b"]);
G2L["d"] = Instance.new("UIGradient", G2L["b"]);
G2L["d"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.690, Color3.fromRGB(255, 255, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 255, 0))};

G2L["e"] = Instance.new("TextLabel", G2L["b"]);
G2L["e"]["BorderSizePixel"] = 0;
G2L["e"]["TextSize"] = 14;
G2L["e"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
G2L["e"]["FontFace"] = Font.new([[rbxasset://fonts/families/Ubuntu.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["e"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["e"]["BackgroundTransparency"] = 1;
G2L["e"]["Size"] = UDim2.new(0, 150, 0, 15);
G2L["e"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["e"]["Text"] = [[Health: 3000/3000]];
G2L["e"]["Name"] = [[health]];
G2L["e"]["Position"] = UDim2.new(-0.07692, 0, 0.982, 0);
G2L["f"] = Instance.new("UIGradient", G2L["e"]);
G2L["f"]["Rotation"] = 90;
G2L["f"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(0, 161, 0))};

G2L["10"] = Instance.new("TextButton", G2L["2"]);
G2L["10"]["BorderSizePixel"] = 0;
G2L["10"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L["10"]["TextSize"] = 14;
G2L["10"]["BackgroundColor3"] = Color3.fromRGB(51, 51, 51);
G2L["10"]["FontFace"] = Font.new([[rbxasset://fonts/families/SourceSansPro.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["10"]["Size"] = UDim2.new(0, 130, 0, 25);
G2L["10"]["Name"] = [[releasebuzz]];
G2L["10"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["10"]["Text"] = [[Release Buzzsaw G]];
G2L["10"]["Position"] = UDim2.new(0.067, 0, 0.464, 0);

lastHpBuzzClone = 3000
local releasedBuzz = Instance.new("BoolValue", G2L["10"])
releasedBuzz.Name = "releasedBuzz"
releasedBuzz.Value = false
G2L["10"].MouseButton1Click:Connect(function()
	releasedBuzz.Value = not releasedBuzz.Value
	if releasedBuzz.Value == true then
		game:GetService("ReplicatedStorage").SFX.OperationSelect:Play()
		G2L["10"]["Text"] = [[Stop Buzzsaw G]]
		G2L["10"].Interactable = false
		local AI_Speed = -5
		local AI_Target = nil
		local AI_Does = false
		pcall(function()
			game.ReplicatedStorage.CombatRemotes.ProjectileSpawn:InvokeServer(game.ReplicatedStorage.MorphShow.QuadBuzzsawGClone, Vector3.new(0, 0, 0))
		end)
		if workspace.Debris:WaitForChild("QuadBuzzsawGClone", 5) then
			local BuzzGHelper = workspace.Debris:WaitForChild("QuadBuzzsawGClone", 5)
			if not workspace:FindFirstChild("CustomAIs") then
				local customAIsFolder = Instance.new("Folder", workspace)
				customAIsFolder.Name = "CustomAIs"
			end
			BuzzGHelper.Parent = workspace.CustomAIs
			local AIFolder = Instance.new("Folder", BuzzGHelper)
			AIFolder.Name = "AI"
			local AIActive = Instance.new("BoolValue", AIFolder)
			AIActive.Name = "AI:Enabled"
			AIActive.Value = true
			local AICombat = Instance.new("StringValue", AIFolder)
			AICombat.Name = "AI:Combat"
			AICombat.Value = "Searching"
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", true, BuzzGHelper)
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("GIVE:TEAM:ASTRO", BuzzGHelper)
			task.spawn(function()
				if BuzzGHelper:WaitForChild("BathroomTeam", 5) and BuzzGHelper ~= nil then
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", BuzzGHelper:FindFirstChild("AstroTeam"), Vector3.new(0,-600,0))
					game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", BuzzGHelper:FindFirstChild("LensmenTeam"), Vector3.new(0,-600,0))
					task.wait(0.2)
					if BuzzGHelper ~= nil then
						game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("IMM", false, BuzzGHelper)
					end
				end
			end)
			game.ReplicatedStorage.CombatRemotes.StunLock:FireServer(BuzzGHelper.PrimaryPart, true)
			game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", BuzzGHelper.PrimaryPart, CFrame.new(game.Players.LocalPlayer.Character.PrimaryPart.Position.X, 230, game.Players.LocalPlayer.Character.PrimaryPart.Position.Z))
			game.Players.LocalPlayer.PlayerScripts.Binds.DoAttackz:Fire("UnionRework", "SonarCore", game.Players.LocalPlayer.Name, {BuzzGHelper.PrimaryPart, "Starrttt"})
			if BuzzGHelper.PrimaryPart:WaitForChild("Starrttt", 5) then
				G2L["10"].Interactable = true
				task.spawn(function()
					while BuzzGHelper and BuzzGHelper.PrimaryPart and AIActive.Value == true do
						local getEnemy = GetNearestPlayer(550, BuzzGHelper.PrimaryPart, "LensmenTeam", false)
						if getEnemy ~= nil then
							if (BuzzGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude <= 100 then
								AICombat.Value = "Attack"
								AI_Target = getEnemy
							elseif (BuzzGHelper.PrimaryPart.Position-getEnemy.PrimaryPart.Position).Magnitude > 100 then
								AICombat.Value = "Chase"
								AI_Target = getEnemy
							end
						else
							AI_Target = nil
							AICombat.Value = "Searching"
						end
						game:GetService("RunService").Heartbeat:Wait()
					end
				end)
				task.spawn(function()
					DoAction = AICombat:GetPropertyChangedSignal("Value"):Connect(function()
						if AI_Does == true then return end
						if AICombat.Value == "Attack" and BuzzGHelper:FindFirstChild("Head") and AI_Target ~= nil and not BuzzGHelper.Head:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(BuzzGHelper.Head, true)
							if BuzzGHelper.Head:WaitForChild("STARMANBUFF", 5) then
								local spinModule = Instance.new("Part", AIFolder)
								spinModule.Name = "post_spin"
								spinModule.Transparency = 1
								spinModule.Anchored = true
								spinModule.CFrame = BuzzGHelper.PrimaryPart.CFrame
								game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(BuzzGHelper.juggernautfast, true)
								if BuzzGHelper ~= nil and AI_Target ~= nil and AI_Target.PrimaryPart then
									game.ReplicatedStorage.CombatRemotes.ParticleSpawn:FireServer(game:GetService("ReplicatedStorage").Assets.Specialized.HALLOWEEN.ghoost, AI_Target.PrimaryPart.CFrame, "saw3")
									game.ReplicatedStorage.CombatRemotes.AIFramework:FireServer("DAMAGE", BuzzGHelper, AI_Target.Humanoid, 500, AI_Target)
								end
								for i = 1, 30 do
									if BuzzGHelper ~= nil and BuzzGHelper.PrimaryPart then
										spinModule.CFrame = spinModule.CFrame * CFrame.Angles(0, math.rad(12), 0)
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", BuzzGHelper.PrimaryPart, spinModule.CFrame)
									end
									task.wait(0.01)
								end
								spinModule:Destroy()
								game.ReplicatedStorage.CombatRemotes.EnableParticle:FireServer(BuzzGHelper.juggernautfast, false)
								task.wait(0.8)
								if BuzzGHelper ~= nil then
									game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(BuzzGHelper.Head, false)
								end
							end
						elseif AICombat.Value == "Chase" and BuzzGHelper:FindFirstChild("HumanoidRootPart") and AI_Target ~= nil and not BuzzGHelper.HumanoidRootPart:FindFirstChild("STARMANBUFF") then
							AI_Does = true
							AI_Speed = -5
							game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(BuzzGHelper.HumanoidRootPart, true)
							if BuzzGHelper.HumanoidRootPart:WaitForChild("STARMANBUFF", 5) then
								if BuzzGHelper and BuzzGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
									game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", BuzzGHelper.PrimaryPart, CFrame.lookAt(BuzzGHelper.PrimaryPart.Position, AI_Target.PrimaryPart.Position) * CFrame.new(0, 0, AI_Speed))
								end
								moveChase = BuzzGHelper.PrimaryPart:GetPropertyChangedSignal("Position"):Connect(function()
									if BuzzGHelper and BuzzGHelper.PrimaryPart and AI_Target ~= nil and AI_Target.PrimaryPart then
										if AI_Speed >= -35 then
											AI_Speed -= 2.5
										end
										game.ReplicatedStorage.CombatRemotes.ClockShuriken:FireServer("cf", BuzzGHelper.PrimaryPart, CFrame.lookAt(BuzzGHelper.PrimaryPart.Position, AI_Target.PrimaryPart.Position) * CFrame.new(0, 0, AI_Speed))
									end	
								end)
								repeat game:GetService("RunService").Heartbeat:Wait() until AICombat.Value ~= "Chase" or BuzzGHelper == nil
								if moveChase then moveChase:Disconnect() end
								if BuzzGHelper ~= nil then
									game.ReplicatedStorage.CombatRemotes.SupermanBuff:FireServer(BuzzGHelper.HumanoidRootPart, false)
								end
							end
						end
					end)
					removedAction = BuzzGHelper.DescendantRemoving:Connect(function(descendant)
						if descendant.Name == "STARMANBUFF" then
							if AICombat then
								AICombat.Value = "None"
							end
							AI_Does = false
						end
					end)
				end)
				task.spawn(function()
					if BuzzGHelper:WaitForChild("Humanoid", 5) then
						if lastHpBuzzClone ~= 3000 then
							game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SET:ZE:OLD:HP", BuzzGHelper.Humanoid, lastHpBuzzClone)
						end
						buzDied = BuzzGHelper.Humanoid.HealthChanged:Connect(function(health)
							G2L["13"]["Text"] = "Health: "..math.floor(health).."/3000"
							if health == 0 then
								G2L["10"].Text = "Signal Lost..." G2L["10"].Interactable = false G2L["12"]:Destroy()
								G2L["14"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.500, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(240, 0, 0))}
								game:GetService("ReplicatedStorage").SFX.ddd:Play()
								repeat task.wait() until AI_Does == false
								AIActive.Value = false
								removedAction:Disconnect()
								DoAction:Disconnect()
								if moveChase then moveChase:Disconnect() end
								AICombat.Value = "Dead"
								G2L["13"].Text = "Dead"
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("TR", BuzzGHelper.skinnedmeshhead, 1)
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SHOULDER:VISIBILITY", BuzzGHelper.skinnedmeshhead, 1)
								buzDied:Disconnect()
								task.wait(5)
								game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", BuzzGHelper, Vector3.new(0,-600,0))
							end
						end)
					end
				end)
			end
		end
	else
		G2L["10"]["Text"] = [[Release Rocketeer G]]
		game:GetService("ReplicatedStorage").SFX.OperationSelect:Play()
		if workspace.CustomAIs:FindFirstChild("QuadBuzzsawGClone") then
			workspace.CustomAIs.QuadBuzzsawGClone.AI["AI:Enabled"].Value = false
			removedAction:Disconnect()
			DoAction:Disconnect()
			if moveChase then moveChase:Disconnect() end
			buzDied:Disconnect()
			workspace.CustomAIs.QuadBuzzsawGClone.AI["AI:Combat"].Value = "CallBack"
			lastHpBuzzClone = math.floor(workspace.CustomAIs.QuadBuzzsawGClone.Humanoid.Health)
			G2L["13"]["Text"] = "Health: "..math.floor(workspace.CustomAIs.QuadBuzzsawGClone.Humanoid.Health).."/3000"
			game.ReplicatedStorage.CombatRemotes.UTTMCombat:FireServer("SMOKEFFEC", workspace.CustomAIs.QuadBuzzsawGClone, Vector3.new(0,-600,0))
		end
	end
end)
G2L["11"] = Instance.new("UICorner", G2L["10"]);
G2L["12"] = Instance.new("UIGradient", G2L["10"]);
G2L["12"]["Color"] = ColorSequence.new{ColorSequenceKeypoint.new(0.000, Color3.fromRGB(255, 255, 255)),ColorSequenceKeypoint.new(0.690, Color3.fromRGB(255, 255, 0)),ColorSequenceKeypoint.new(1.000, Color3.fromRGB(255, 255, 0))};

