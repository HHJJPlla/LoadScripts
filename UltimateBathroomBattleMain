function GetNearestPlayer()
    local nearestPlayer = nil
    local shortestDistance = 8000
    for _, v in pairs(game.Players:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
            if v.Character ~= nil and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("LensmenTeam") then
                local distance = (workspace.GManBathroom2.HumanoidRootPart.Position - v.Character:FindFirstChild("HumanoidRootPart").Position).Magnitude
                if distance < shortestDistance then
                    nearestPlayer = v
                end
            end
        end
    end
    return nearestPlayer
end

